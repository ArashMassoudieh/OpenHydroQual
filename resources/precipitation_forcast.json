{
  "solutionorder": [ "Storage" ],
  "Precipitation_w_forcast": {
    "type": "source",
    "typecategory": "Sources",
    "description": "Precipitation with forcast",
    "icon": {
      "filename": "cloud.png"
    },
    "coefficient": {
      "type": "expression",
      "expression": "area",
      "ask_user": "false",
      "delegate": "ValueBox"
    },
    "timeseries": {
      "type": "timeseries_prec",
      "includeinoutput": "true",
      "description": "Precipitation intensity",
      "ask_user": "true",
      "delegate": "Browser;time series"
   },
   "kernel_smoothed_forcast": {
	"type": "expression",
	"expression": "_ekr(timeseries;0.5)",
	"description": "Kernel Smoothed Forcast",
	"includeinoutput": "true",
        "ask_user": "false"
   }
  },
  "Flow_control_device": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Flow control device",
    "icon": {
      "filename": "reservoir_link.png"
    },
    "timeseries": {
      "type": "timeseries_prec",
      "includeinoutput": "true",
      "description": "Precipitation intensity",
      "ask_user": "true",
      "delegate": "Browser;time series"
   },
   "kernel_smoothed_forcast": {
	"type": "expression",
	"expression": "_ekr(timeseries;0.5)",
	"description": "Kernel Smoothed Forcast",
	"includeinoutput": "true",
        "ask_user": "false"
   },
    "flow": {
      "type": "rule",
      "description": "Flow",
      "rule": {
	      "kernel_smoothed_forcast<S_min": "Q_min",
	      "S_min<kernel_smoothed_forcast<S_max": "Q_min+((kernel_smoothed_forcast-S_min)/(S_max-S_min))*(Q_max-Q_min)",
	      "kernel_smoothed_forcast>S_max": "Q_max"
    	},
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    },
    "Q_min": {
      "type": "value",
      "default": "10",
      "description": "Minimum flow",
      "delegate": "UnitBox",
      "unit": "m~^3/day;ft~^3/day",
      "ask_user": "true",
      "estimate": "true"
    },
    "Q_max": {
      "type": "value",
      "default": "100",
      "description": "Maximum flow",
      "delegate": "UnitBox",
      "unit": "m~^3/day;ft~^3/day",
      "ask_user": "true",
      "estimate": "true"
    },
    "S_min": {
      "type": "value",
      "default": "0",
      "description": "Lower precipitation threshold",
      "delegate": "UnitBox",
      "unit": "m/day",
      "ask_user": "true",
      "estimate": "true"
    },
    "S_max": {
      "type": "value",
      "default": "100",
      "description": "Upper precipitation threshold",
      "delegate": "UnitBox",
      "unit": "m/day",
      "ask_user": "true",
      "estimate": "true"
    }
  }
}
