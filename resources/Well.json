{
	"Well": {
		"type": "block",
		"typecategory": "Blocks",
                "description": "Well",
		"icon": {
			"filename": "Well.png"		
		},
		"Storage": {
			"description": "Storage",
                        "type": "balance",
                        "massbalance": "true",
                        "flow": "flow",
                        "initial_value_expression": "(diameter^2)/4*3.1415*depth",
                        "ask_user": "false",
                        "delegate": "ValueBox",
                        "estimate": "false",
                        "includeinoutput" : "true"
		},
		"diameter": {
			"type": "value",
	      		"description": "Well Diameter",
	      		"ask_user": "true",
	      		"delegate": "ValueBox",
	      		"default": "1"
		},
		"area": {
			"type": "expression",
			"description": "Well cross-sectional area",
			"expression": "(diameter^2)/4*3.1415",
			"description": "Cross-sectional area",
			"includeinoutput": "true"
		},
		"depth": {
			 "type": "expression",
			 "expression" : "Storage/area",
			 "description": "Water depth",
			 "ask_user": "true",
			 "includeinoutput": "true",
  		         "delegate": "ValueBox"  
		},
		"bottom_elevation": {
		     "type": "value",
		      "description": "Bottom elevation of the well",
		      "ask_user": "true",
		      "delegate": "ValueBox"
		},
		"head": {
		 	"type": "expression",
		        "expression": "depth+bottom_elevation",
		        "description" : "Hydraulic head",
		        "ask_user": "false",
		        "includeinoutput" : "true"
		},
              "x": {
		      "type": "value",
		      "description": "x",
		      "default": "900",
		      "ask_user": "true",
		      "delegate": "ValueBox"
		    },
		"y": {
		      "type": "value",
		      "description": "y",
		      "default": "900",
		      "ask_user": "true",
		      "delegate": "ValueBox"
		    },
		    "_width": {
		      "type": "value",
		      "description": "_width",
		      "default": "200",
		      "ask_user": "true",
		      "delegate": "ValueBox"
		    },
		    "_height": {
		      "type": "value",
		      "description": "_height",
		      "default": "200",
		      "ask_user": "true",
		      "delegate": "ValueBox"
                    }
	},
        "Well2soil horizontal link":{
		"type": "link",
		"typecategory": "Connectors",
                "description": "Horizontal link from well to soil",
		"icon": {
			"filename": "well-to-soil-H-link.png"
		},
		"name": {
		      "description": "Name",
		      "type": "string",
		      "ask_user": "true",
		      "delegate": "String" 
		},
                "color": {
      			"type": "string",
      			"default": "60,120,60"
    		},
                "length" : {
			"type": "value",
			"description": "Connector length",
			"ask_user" : "true",
			"delegate" : "ValueBox",
			"estimate" : "true",
			"length": ">0",
		        "warningmessage": "length must be positive"
		},
		"interface_area":{
			"type": "expression",
			"description":"Interface area between well and soil/gw",
			"expression":"(_min(_max(0;head.s-bottom_elevation.e);depth.e)+_hsd(pressure_head.e)*_max(_min(depth.e+bottom_elevation.e-head.s;depth.e);0))*3.14*diameter.s",
			"includeinoutput" : "true"
		},
		"hydraulic_gradient":{
			"type": "expression",
			"description":"Hydraulic Gradient",
			"expression":"(head.s-head.e)/length",
			"includeinoutput" : "true"
		},	
	        "flow" : {
		      	"type": "expression",
			"description": "Flow rate",
                        "expression" : "K_sat.e*hydraulic_gradient*interface_area",
			"includeinoutput" : "true",
			"applylimit": "true"	
		}  	
	},
        "Well2soil vertical link":{
		"type": "link",
		"typecategory": "Connectors",
                "description": "Vertical link from well to soil",
		"icon": {
			"filename": "well-to-soil-V-link.png"
		},
		"name": {
		      "description": "Name",
		      "type": "string",
		      "ask_user": "true",
		      "delegate": "String" 
		},
                "color": {
      			"type": "string",
      			"default": "60,220,60"
    		},
                "length" : {
			"type": "expression",
			"description": "Connector length",
			"expression": "depth.e/2",
			"ask_user" : "false"
		},
		"hydraulic_gradient":{
			"type": "expression",
			"description":"Hydraulic Gradient",
			"expression":"(head.s-head.e)/length",
			"includeinoutput" : "true"
		},	
	        "flow" : {
		      	"type": "expression",
			"description": "Flow rate",
                        "expression" : "K_sat.e*hydraulic_gradient*area.s",
                        "description": "Flow (m~^3/day)",
			"includeinoutput" : "true",
			"applylimit": "true"	
		}  	
	},
	"Surface water to well":{
		"type": "link",
		"typecategory": "Connectors",
                "description": "Surface water to well link",
		"icon": {
			"filename": "surface2well.png"
		},
		"name": {
		      "description": "Name",
		      "type": "string",
		      "ask_user": "true",
		      "delegate": "String" 
		},
                "color": {
      			"type": "string",
      			"default": "180,120,40"
    		},
                "length" : {
			"type": "value",
			"description": "Connector length",
			"ask_user" : "true",
			"delegate" : "ValueBox",
			"estimate" : "true",
			"length": ">0",
		        "warningmessage": "length must be positive"
		},
		"ManningCoeff": {
			"type": "value",
			"description": "Manning's roughness coefficient",
			"ask_user" : "true",
			"delegate" : "ValueBox",
			"includeinoutput": "false",
			"default": "0.1",
			"ManningCoeff": ">0",
		        "warningmessage": "ManningCoeff must be positive"
		},
		"flow": {
			"type": "expression",
			"expression": "86400*(_sqt((head.s-head.e)/length)/ManningCoeff)*3.14*diameter.e*depth.s^1.6667",
			"description": "Flow (m~^3/day)",
			"includeinoutput": "true",
			"applylimit": "true",
			"ask_user": "false"
		}  	
	}
}
