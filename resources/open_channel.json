  "Sewer_channel_segment": {
    "type": "block",
    "typecategory": "Blocks",
    "description": "Sewer Channel Segment",
    "icon": {
      "filename": "Sewer_pipe.png"
    },
    "Storage": {
      "type": "balance",
      "massbalance": "true",
      "flow": "flow",
      "inflow": "inflow,ag_withdrawal,non_ag_withdrawal",
      "includeinoutput": "true",
      "description": "Storage",
      "initial_value_expression": "length*diameter^2*(-0.910331972*(depth/diameter)^3+1.376045712*(depth/diameter)^2+0.319684423*(depth/diameter))",
      "ask_user": "false",
      "estimate": "true",
      "unit": "m^3;ft^3"
    },
    "ManningCoeff": {
      "type": "value",
      "description": "Manning's Roughness Coefficient",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true"
    },
    "bottom_elevation": {
      "type": "value",
      "description": "Bottom Elevation",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm",
      "estimate": "true"
    },
    "diameter": {
      "type": "value",
      "description": "Diameter",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "estimate": "true",
      "criteria": "diameter>0",
      "warningmessage": "Diameter cannot be zero"
    },
    "depth": {
      "type": "expression",
      "expression": "normalized_depth*diameter",
      "precalcbasedon": "Storage:log:0.000001:1000000",
      "description": "Initial Water Depth; Water Depth",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm",
      "includeinoutput": "true",
      "criteria": "depth>-0.0000001",
      "warningmessage": "Depth must be non-negative",
      "unit": "m;ft"
    },
    "normalized_area": {
      "type": "expression",
      "expression": "Storage/(length*diameter^2)",
      "description": "Normalized area",
      "ask_user": "false"
    },
    "normalized_depth": {
      "type": "expression",
      "expression": "2.2298958*normalized_area^3-2.7110899*normalized_area^2+2.027012721*normalized_area",
      "description": "Normalized water depth",
      "ask_user": "false"
    },
    "head": {
      "type": "expression",
      "expression": "depth+bottom_elevation+_pos(depth-diameter)*10000",
      "description": "Hydraulic Head",
      "ask_user": "false",
      "includeinoutput": "true",
      "unit": "m;ft"
    },
    "length": {
      "type": "value",
      "description": "Segment length",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm",
      "estimate": "true",
      "criteria": "length>0",
      "warningmessage": "Length must be greater than zero"
    },
    "area": {
      "type": "expression",
      "expression": "diameter^2*(-0.910331972*(depth/diameter)^3+1.376045712*(depth/diameter)^2+0.319684423*(depth/diameter))",
      "description": "Cross-sectional area",
      "ask_user": "false",
      "includeinoutput": "true",
      "unit": "m^2;ft^2"
    },
    "perimeter": {
      "type": "expression",
      "expression": "diameter*(5.486583*(depth/diameter)^3-8.60202*(depth/diameter)^2+6.1081605*(depth/diameter))",
      "description": "Wetted Perimeter",
      "ask_user": "false",
      "includeinoutput": "true",
      "unit": "m;ft"
    },
    "inflow": {
      "type": "timeseries",
      "includeinoutput": "true",
      "applylimit": "true",
      "description": "Inflow time series",
      "ask_user": "true",
      "delegate": "Browser",
      "unit": "m^3/day;m^3/s;ft^3/s"
    }
  },
  "Leaky Dam": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Leaky Dam",
    "icon": {
      "filename": "LeakyDam.png"
    },
    "Width": {
      "type": "value",
      "description": "Width",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m",
      "estimate": "true"
    },
    "Hydraulic_Conductivity": {
      "type": "value",
      "description": "Hydraulic Conductivity",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m/day",
      "estimate": "true"
    },
    "Length": {
      "type": "value",
      "description": "Dam Length in the flow direction",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m",
      "estimate": "true",
      "default": "1"
    },
    "Weir_coefficient": {
      "type": "value",
      "description": "Weir Coefficient",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true",
      "default": "0.85"
      },
      "Dam_Height": 
      {
      	"type": "value",
      	"description": "Dam Height",
      	"ask_user": "true",
      	"delegate": "UnitBox",
      	"unit": "m",
      	"estimate": "true"
      },
      "Hydraulic_Gradient": 
      {
      	"type": "expression",
      	"description": "Hydraulic Gradient",
      	"expression": "(head.s-head.e)/Length",
      	"includeinoutput": "true",
      	"ask_user": "false"
      },
      "under_flow":
      {
      	"type": "expression",
      	"description": "Under flow",
      	"expression": "Width*Hydraulic_Conductivity*Hydraulic_Gradient*_max(depth.s;Dam_Height)",
      	"includeinoutput": "true",
      	"ask_user": "false",
      	"unit": "m^3/day;ft^3/s"
      },
      "over_flow": 
      {
      	"type": "expression",
      	"description": "Over flow",
