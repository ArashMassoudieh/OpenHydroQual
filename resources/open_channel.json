{
  "Trapezoidal Channel Segment": {
    "type": "block",
    "typecategory": "Blocks",
    "icon": {
      "filename": "Trapezoidal_Channel.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String"
    },
    "Storage": {
      "type": "balance",
      "massbalance": "true",
      "flow": "flow",
      "inflow": "inflow",
      "includeinoutput": "true",
      "description": "Storage",
      "initial_value_expression": "length*(base_width+side_slope*depth)*depth",
      "ask_user": "false",
      "delegate": "ValueBox",
      "estimate": "true"
    },
    "base_width": {
      "type": "value",
      "description": "Base Width",
      "ask_user": "true",
      "delegate": "ValueBox",
      "unit": "m",
      "estimate": "true"
    },
    "side_slope": {
      "type": "value",
      "description": "Side Slope",
      "ask_user": "true",
      "delegate": "ValueBox",
      "unit": "",
      "estimate": "true",
      "criteria": "side_slope>0",
      "warningmessage": "Side slope must be greater than zero"
    },
    "x": {
      "type": "value",
      "description": "x",
	    "default": "900",
      "ask_user": "true",
      "delegate": "ValueBox"
    },
    "y": {
      "type": "value",
      "description": "y",
      "default": "900",
      "ask_user": "true",
      "delegate": "ValueBox"
    },
    "_width": {
      "type": "value",
      "description": "_width",
      "default": "200",
      "ask_user": "true",
      "delegate": "ValueBox"
    },
    "_height": {
      "type": "value",
      "description": "_height",
	  "default": "200",
      "ask_user": "true",
      "delegate": "ValueBox"
    },
    "ManningCoeff": {
      "type": "value",
      "description": "Manning's Roughness Coefficient",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true"
    },
    "bottom_elevation": {
      "type": "value",
      "description": "Bottom Elevation",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true"
    },
    "depth": {
      "type": "expression",
      "expression": "(-base_width+_sqt(base_width^2+4*side_slope*Storage/length))/(2*side_slope)",
      "precalcbasedon": "Storage:log:0.000001:1000000",
      "description": "Water Depth",
      "ask_user": "true",
      "delegate": "ValueBox",
      "includeinoutput": "true",
      "criteria": "depth>-0.0000001",
      "warningmessage": "Depth must be non-negative"
    },
    "head": {
      "type": "expression",
      "expression": "depth+bottom_elevation",      
      "description": "Hydraulc Head",
      "ask_user": "false",
      "delegate": "ValueBox",
      "includeinoutput": "true"
    },
    "length": {
      "type": "value",
      "description": "Segment length",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true",
      "criteria": "length>0",
      "warningmessage": "Length must be greater than zero"
    },
    "area": {
      "type": "expression",
      "expression": "(base_width+side_slope*depth)*depth",
      "description": "Cross-sectional area",
      "ask_user": "false",
      "delegate": "ValueBox",
      "includeinoutput": "true"
    },
    "perimeter": {
      "type": "expression",
      "expression": "base_width+2*_sqt(1+side_slope^2)*depth",
      "description": "Wetted Perimeter",
      "ask_user": "false",
      "delegate": "ValueBox",
      "includeinoutput": "true"
    },
    "inflow": {
      "type": "timeseries",
      "includeinoutput": "true",
      "applylimit": "true",
      "description": "Inflow time series",
      "ask_user": "true",
      "delegate": "Browser"
    }
  },
  "Trapezoidal_Channel_link": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Link from a channel segment to another",
    "icon": {
      "filename": "Trapezoidal_Channel_Link.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "area": {
      "type": "expression",
      "description": "Cross-sectional area",
      "expression": "_max(_bkw(head;area);0)",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "perimeter": {
      "type": "expression",
      "description": "Wetted Perimeter",
      "expression": "(perimeter.s+perimeter.e)/2",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "hydraulic_radious": {
      "type": "expression",
      "description": "Hydraulic Radius",
      "expression": "area/perimeter",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "ManningCoeff": {
      "type": "expression",
      "description": "Manning's roughness coefficient",
      "expression": "(ManningCoeff.s+ManningCoeff.e)/2",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "length": {
      "type": "expression",
      "description": "Connector length",
      "expression": "(length.s+length.e)/2",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "flow": {
      "type": "expression",
      "description": "Flow",
      "expression": "86400*(_sqt((head.s-head.e)/length)/ManningCoeff)*area*hydraulic_radious^0.6667",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    }
  },
  "Fixedhead_to_channel": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Link from a fixed head boundary to a channel segment",
    "icon": {
      "filename": "fixed2channel.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "area": {
      "type": "expression",
      "description": "Cross-sectional area",
      "expression": "(base_width.e+side_slope.e*_pos(head.s-bottom_elevation.e))*_pos(head.s-bottom_elevation.e)",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "perimeter": {
      "type": "expression",
      "description": "Wetted Perimeter",
      "expression": "base_width.e+2*_sqt(1+side_slope.e^2)*_pos(head.s-bottom_elevation.e)",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "hydraulic_radious": {
      "type": "expression",
      "description": "Hydraulic Radius",
      "expression": "area/perimeter",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "ManningCoeff": {
      "type": "expression",
      "description": "Manning's roughness coefficient",
      "expression": "ManningCoeff.e",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "length": {
      "type": "expression",
      "description": "Connector length",
      "expression": "length.e",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "flow": {
      "type": "expression",
      "description": "Flow",
      "expression": "86400*(_sqt((head.s-head.e)/length)/ManningCoeff)*area*hydraulic_radious^0.6667",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    }
  },
  "channel2fixed": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Link from a channel segment to fixed head boundary",
    "icon": {
      "filename": "channel2fixed.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "area": {
      "type": "expression",
      "description": "Cross-sectional area",
      "expression": "_max(area.s;0)",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "perimeter": {
      "type": "expression",
      "description": "Wetted Perimeter",
      "expression": "perimeter.s",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "hydraulic_radious": {
      "type": "expression",
      "description": "Hydraulic Radius",
      "expression": "area/perimeter",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "ManningCoeff": {
      "type": "expression",
      "description": "Manning's roughness coefficient",
      "expression": "ManningCoeff.s",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "length": {
      "type": "expression",
      "description": "Connector length",
      "expression": "length.s",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "flow": {
      "type": "expression",
      "description": "Flow",
      "expression": "86400*(_sqt((head.s-head.e)/length)/ManningCoeff)*area*hydraulic_radious^0.6667",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    }
  }
}
