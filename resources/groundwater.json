{
  "Groundwater cell": {
    "type": "block",
    "typecategory": "Blocks",
    "icon": {
      "filename": "Groundwater.png"
    },
    "Storage": {
      "type": "balance",
      "massbalance": "true",
      "flow": "flow",
      "inflow": "Precipitation",
      "includeinoutput": "true",
      "description": "Initial Storage; Storage",
      "initial_value_expression": "area*depth*moisture_content",
      "ask_user": "false",
      "estimate": "true"
    },
    "hydraulic_conductivity": {
      "type": "value",
      "description": "Hydraulic Conductivity",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m/day;ft/day;cm/s;in/s;m/s",
      "estimate": "true",
      "default": "1",
      "criteria": "hydraulic_conductivity>0",
      "warningmessage": "Hydraulic conductivity must be greater than zero"
    },
    "K_eff": {
      "type": "expression",
      "expression": "hydraulic_conductivity",      
      "description": "Hydraulic conductivity",
      "ask_user": "false",
      "includeinoutput": "false"
    },
    "porosity": {
      "type": "value",
      "description": "Porosity",
      "ask_user": "true",
      "default": "0.4",
      "delegate": "ValueBox",
      "estimate": "true"
    },
    "specific_storage": {
      "type": "value",
      "description": "Specific Storage",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "1/m;1/ft;1/cm;1/in",
      "estimate": "true",
      "default": "0.01",
      "criteria": "specific_storage>0",
      "warningmessage": "Specific Storage must be greater than zero"
    },
    "bottom_elevation": {
      "type": "value",
      "description": "Bottom Elevation",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "estimate": "true"
    },
    "depth": {
      "type": "value",
      "description": "Cell Depth",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "includeinoutput": "false",
      "criteria": "depth>0",
      "warningmessage": "Cell depth must be greater than zero"
    },
    "moisture_content": {
      "type": "expression",
      "expression": "Storage/(depth*area)",      
      "description": "Moisture content",
      "ask_user": "true",
      "delegate": "ValueBox",
      "includeinoutput": "true"
    },
    "head": {
      "type": "expression",
      "expression": "depth+bottom_elevation+(moisture_content-porosity)/specific_storage",      
      "description": "Hydraulc Head",
      "ask_user": "false",
      "includeinoutput": "true"
    },
    "area": {
      "type": "value",
      "description": "Cell area",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m~^2;ft~^2;cm~^2;in~^2",
      "includeinoutput": "false",
      "criteria": "area>0",
      "warningmessage": "Cell area must be greater than zero"
    }
  },
  "groundwater_link": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Groundwater link",
    "icon": {
      "filename": "Groundwater_link.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "length": {
      "type": "value",
      "description": "Length",
      "includeinoutput": "false",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "criteria": "length>0",
      "warningmessage": "Length must be greater than zero"
    },
    "area": {
      "type": "value",
      "description": "Interface Area",
      "includeinoutput": "false",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m~^2;ft~^2;cm~^2;in~^2",
      "criteria": "area>0",
      "warningmessage": "Interface area must be greater than zero"
    },
    "flow": {
      "type": "expression",
      "expression": "0.5*area*(hydraulic_conductivity.s+hydraulic_conductivity.e)*(head.s-head.e)/length",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    }
  },
  "groundwater_to_fixedhead": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Groundwater to fixed head",
    "icon": {
      "filename": "gw_2_fixed_head.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "length": {
      "type": "value",
      "description": "Length",
      "includeinoutput": "false",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "criteria": "length>0",
      "warningmessage": "Length must be greater than zero"
    },
    "area": {
      "type": "value",
      "description": "Interface Area",
      "includeinoutput": "false",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m~^2;ft~^2;cm~^2;in~^2",
      "criteria": "area>0",
      "warningmessage": "Interface area must be greater than zero"
    },
    "flow": {
      "type": "expression",
      "description": "Flow",
      "expression": "area*hydraulic_conductivity.s*(head.s-head.e)/length",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    }
  },
   "groundwater_to_stream": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Groundwater to stream",
    "icon": {
      "filename": "gw_2_fixed_head.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "length": {
      "type": "value",
      "description": "Length",
      "includeinoutput": "false",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "criteria": "length>0",
      "warningmessage": "Length must be greater than zero"
    },
    "area": {
      "type": "value",
      "description": "Interface Area",
      "includeinoutput": "false",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m~^2;ft~^2;cm~^2;in~^2",
      "criteria": "area>0",
      "warningmessage": "Interface area must be greater than zero"
    },
    "flow": {
      "type": "expression",
      "description": "Flow",
      "expression": "area*hydraulic_conductivity.s*(head.s-head.e)/length",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    }
  },
  "soil2groundwater_link": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Soil to groundwater link",
    "icon": {
      "filename": "soil2gw_link.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "length": {
      "type": "expression",
      "expression": "(depth.s+depth.e)/2",
      "description": "Length",
      "includeinoutput": "false",
      "ask_user": "false"
    },
    "area": {
      "type": "value",
      "description": "Interface Area",
      "includeinoutput": "false",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m~^2;ft~^2;cm~^2;in~^2",
      "criteria": "area>0",
      "warningmessage": "Interface area must be greater than zero"
    },
    "flow": {
      "type": "expression",
      "description": "Flow",
      "expression": "0.5*area*(K_sat.s+hydraulic_conductivity.e)*(head.s-head.e)/length",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    }
  },
