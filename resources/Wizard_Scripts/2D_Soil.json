{
  "name": "2DSoilGrid",
  "icon": "soil_grid.png",
  "description": "This wizard creates a two-dimentional vertical (2DV) soil grid",
  "addtemplate": ["unsaturated_soil.json"],
  "major_block": {
    "name": "Soil",
    "type": "Soil",
    "v_connector_type": "Soil2Soil_connector",
    "h_connector_type": "Soil2Soil_connector_H",
    "n_x": "param[n_x]",
    "n_y": "param[n_y]",
    "K_s": "param[K_s]",
    "alpha": "1"
  },
  "major_block": {
    "name": "SubSoil",
    "type": "Soil",
    "v_connector_type": "Soil2Soil_connector",
    "h_connector_type": "Soil2Soil_connector_H",
    "n_x": "param[n_x]",
    "n_y": "param[n_y]",
    "K_s": "param[K_s]",
    "alpha": "1"
  },
  "parameter": {
    "name": "n_x",
    "delegate": "ValueBox",
    "question": "Number of cells in horizontal direction"
  },
  "parameter": {
    "name": "n_y",
    "delegate": "ValueBox",
    "question": "Number of cells in vertical direction"
  },
  "parameter_group": {
    "name": "g1",
    "description": "Introduction",
    "parameters": [
      "n_x",
      "n_y"
    ],
    "criteria": [
      {
        "name": "cr1",
        "less": "0",
        "greater": "Wind_ceoff",
        "ErrorMessage": "Wind coefficient must be positive"
      }
    ]
  }
}
