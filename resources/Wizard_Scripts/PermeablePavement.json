{
  "name": "PermeablePavement (PP)",
  "icon": "PermeablePavement.png",
  "description": "This wizard create a Permeable Pavement ",
  "diagram": "Permeable_Pavement2.svg",
  "addtemplate": [
    "main_components.json",
    "Pond_Plugin.json",
    "groundwater.json",
    "unsaturated_soil.json",
    "Sewer_system.json"
  ],
  "entities": [
    {
      "name": "Rain",
      "entity": "source",
      "type": "Precipitation",
      "timeseries": "TimeSeriesFileName@sp"
    }
  ],
  "setvals": [
    {
      "name": "start_date",
      "object": "system@sc",
      "quantity": "simulation_start_time@sc",
      "value": "StartTime@dp"
    },
    {
      "name": "end_date",
      "object": "system@sc",
      "quantity": "simulation_end_time@sc",
      "value": "EndTime@dp"
    }
  ],
  "singleblocks": [
    {
      "name": "Contributing impervious catchment",
      "area": "lenght*width;m~^2",
      "Precipitation": "Rain@sc",
      "type": "Catchment",
      "Slope": "Catchment_slope",
      "ManningCoeff": "Catchment_Manning_coefficient",
      "Width": "Catchment_mean_width",
      "x": "-26.66",
      "y": "-650.80",
      "_width": "300",
      "_height": "300"
    },
    {
      "name": "Ponded water",
      "area": "lenght*width;m~^2",
      "Precipitation": "Rain@sc",
      "type": "Catchment",
      "Slope": "Catchment_slope",
      "ManningCoeff": "Catchment_Manning_coefficient",
      "Width": "Catchment_mean_width",
      "x": "922.616",
      "y": "-498.05",
      "_width": "300",
      "_height": "300"
    },
    {
      "name": "Aggregate storage",
      "type": "Aggregate_storage_layer",
      "x": "1000",
      "y": "750",
      "bottom_elevation": "-eng_soil_height-storage_layer_height;m",
      "porosity": "aggregate_porosity",
      "area": "lenght*width;m~^2",
      "_width": "200",
      "_height": "200"
    },
    {
      "name": "Permeable pavement",
      "type": "Groundwater cell",
      "area": "lenght*width;m~^2",
      "Width": "",
      "x": "943.82",
      "y": "53.95",
      "_width": "300",
      "_height": "300"
    },
    {
      "name": "Ground_Water",
      "type": "fixed_head",
      "x": "1050",
      "y": "1000 + native_soil_n_y*250",
      "_width": "100",
      "_height": "100",
      "head": "-depth_to_groundwater"
    },
    {
      "name": "Receiving_sewer_pipe",
      "type": "fixed_head",
      "x": "1913",
      "y": "336",
      "_width": "200",
      "_height": "200",
      "head": "-Catch_Basin_Depth - 0.2;m"
    },
    {
      "name": "Catch basin",
      "type": "Catch basin",
      "bottom_elevation": "-Catch_Basin_Depth;m",
      "area": "Catch_Basin_Area",
      "x": "1789.71",
      "y": "-179.08",
      "_width": "200",
      "_height": "200"
    }
  ],
  "blockarrays": [
    {
      "name": "Native soil",
      "type": "Soil",
      "grid_type": "2D-Rect",
      "v_connector_type": "soil_to_soil_link",
      "h_connector_type": "soil_to_soil_H_link",
      "n_x": "1",
      "n_y": "native_soil_n_y",
      "K_sat_original": "native_soil_K_s;m/day",
      "K_sat_scale_factor": "1",
      "depth": "(depth_to_groundwater-eng_soil_height-storage_layer_height)/native_soil_n_y;m",
      "alpha": "native_soil_alpha;1/m",
      "n": "native_soil_n",
      "area": "lenght*width;m~^2",
      "bottom_elevation": "surface_elevation-eng_soil_height-storage_layer_height-(j+1)*(depth_to_groundwater-eng_soil_height-storage_layer_height)/native_soil_n_y;m",
      "_width": "200",
      "_height": "200",
      "specific_storage": "0.01;1/m",
      "theta": "0.2",
      "theta_res": "native_soil_theta_res",
      "theta_sat": "native_soil_theta_sat",
      "x": "1000",
      "y": "1000+j*250"
    }
  ],
  "connectors": [
    {
      "name": "Catchment to Ponded water",
      "from": "Contributing impervious catchment",
      "to": "Ponded water",
      "type": "Catchment_link",
      "connectiontype": "o2o",
      "connectionconfig": "u2d"
    },
    {
      "name": "Aggregate storage to Native soil",
      "from": "Aggregate storage",
      "to": "Native soil",
      "type": "aggregate_to_soil_link",
      "connectiontype": "o2m",
      "connectionconfig": "u2d"
    },
    {
      "name": "Soil to Groundwater center",
      "from": "Native soil",
      "to": "Ground_Water",
      "type": "soil_to_fixedhead_link",
      "connectiontype": "m2o",
      "connectionconfig": "u2d"
    },
    {
      "name": "Ponded water to Catch basin ",
      "from": "Ponded water",
      "to": "Catch basin",
      "type": "Catchment_link",
      "connectiontype": "o2o",
      "connectionconfig": "u2d"
    },
    {
      "name": "Aggregate storage to Catch basin",
      "from": "Aggregate storage",
      "to": "Catch basin",
      "type": "Sewer_pipe",
      "connectiontype": "o2o",
      "connectionconfig": "u2d"
    },
    {
     "name": "Ponded water to Permeable pavement",
     "from": "Ponded water",
     "to": "Permeable pavement",
     "type":"surface2groundwater_link",
     "connectiontype": "o2o",
     "connectionconfig": "u2d"
    },
    {
    
     "name": "Permeable pavement to Aggregate storage",
     "from": "Permeable pavement",
     "to": "Aggregate storage",
     "type":"surface2groundwater_link",
     "connectiontype": "o2o",
     "connectionconfig": "u2d"
    },
    {
      "name": "Catch basin to Receiving_sewer_pipe",
      "from": "Catch basin",
      "to": "Receiving_sewer_pipe",
      "type": "Sewer_pipe",
      "connectiontype": "o2o",
      "connectionconfig": "u2d"
    }
  ],
  "parameters": [
   {
      "name": "native_soil_n_y",
      "delegate": "SpinBox",
      "question": "Number of cells layers native",
      "range": [
        "1",
        "10"
      ],
      "default": "3"
    },
    {
      "name": "native_soil_K_s",
      "delegate": "UnitBox",
      "question": "Nagtive soil hydraulic conductivity",
      "unit": "m/day",
      "default": "0.2"
    },
    {
      "name": "native_soil_alpha",
      "delegate": "UnitBox",
      "question": "Native soil Van Genuchten Î± parameter",
      "unit": "1/m",
      "default": "1"
    },
    {
      "name": "native_soil_n",
      "delegate": "ValueBox",
      "question": "Native soil Van Genuchten n parameter",
      "default": "1.41"
    },
    {
      "name": "native_soil_theta_res",
      "delegate": "ValueBox",
      "question": "Native soil residual moisture content",
      "default": "0.05"
    },
    {
      "name": "native_soil_theta_sat",
      "delegate": "ValueBox",
      "question": "Native soil saturated moisture content",
      "default": "0.4"
    },
    {
     "name": "lenght",
     "delegate": "UnitBox",
     "question":"Lenght",
     "default": "100",
     "unit": "m~^2"
    },
    {
     "name": "width",
     "delegate": "UnitBox",
     "question":"Width ",
     "default": "100",
     "unit": "m~^2"
    },
    {  
      "name": "TimeSeriesFileName",
      "delegate": "FileBrowser",
      "question": "Precipitation file name"
    },
    {
      "name": "StartTime",
      "delegate": "DateBox",
      "question": "Simulation Start Date"
    },
    {
      "name": "EndTime",
      "delegate": "DateBox",
      "question": "Simulation End Date"
    },
    {
      "name": "Catchment_area",
      "delegate": "UnitBox",
      "question": "The area of impervious contributing area",
      "unit": "m~^2",
      "default": "1000"
    },
    {
      "name": "Catchment_slope",
      "delegate": "ValueBox",
      "question": "The slope of the impervious contributing area",
      "default": "0.01"
    },
    {
      "name": "Catchment_Manning_coefficient",
      "delegate": "ValueBox",
      "question": "Effective Manning's roughness coefficient of the impervious contributing area",
      "default": "0.011"
    },
    {
      "name": "Catchment_mean_width",
      "delegate": "UnitBox",
      "question": "Catchment average width",
      "unit": "m",
      "default": "30"
    } , 
       {
      "name": "storage_layer_height",
      "delegate": "UnitBox",
      "question": "Depth of the Aggregate soil",
      "unit": "m",
      "default": "0.4"
    },
     {
      "name": "aggregate_porosity",
      "delegate": "ValueBox",
      "question": "Porosity",
      "default": "0.4"
    },
     {
      "name": "ManningCoeff_aggregate",
      "delegate": "ValueBox",
      "question": "Manning's roughness coefficient of the drainage pipe ",
      "default": "0.01"
    },
    {
     "name": "diameter_aggregate",
      "delegate": "UnitBox",
      "question": "Underdrain line diameter from aggregate soil to catch basin",
      "unit": "m",
      "default": "0.2"
    },
    {
      "name": "length_aggregate",
      "delegate": "UnitBox",
      "question": "Underdrain length from aggregate soil to catch basin",
      "unit": "m",
      "default": "20"
    }
  ],
  "parameter_groups": [
   {
      "name": "1_general",
      "description": "General Model parameters",
      "parameters": [
		"StartTime",
		"EndTime"
      ]
    },
    {
    "name": "4_Sewer",
    "description": "Sewer pipe properties ",
    "parameters": [
      "ManningCoeff_aggregate",
      "diameter_aggregate",
      "length_aggregate"
     ]
    },
  {
      "name": "2_geometry",
      "description": "Model geometry",
      "parameters": [
        "native_soil_n_y",
        "depth_to_groundwater",
        "width",
        "lenght",
        "Catchment_area",
        "Catchment_slope",
        "Catchment_Manning_coefficient",
        "Catchment_mean_width",
        "TimeSeriesFileName"
     ]
    },
    {
      "name": "3_Aggregate storage",
      "description": "Soil aggregate properties",
      "parameters": [
        "storage_layer_height",
        "aggregate_porosity"
      ]
    }
  ],
  "criteria": [
    {},
    {}
  ]
}
