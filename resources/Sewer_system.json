{
  "Sewerchannelsegment": {
    "type": "block",
    "typecategory": "Blocks",
    "description": "Sewer Channel Segment",
    "icon": {
      "filename": "Sewer_pipe_section.png"
    },
    "Storage": {
      "type": "balance",
      "massbalance": "true",
      "flow": "flow",
      "inflow": "inflow",
      "includeinoutput": "true",
      "description": "Storage",
      "initial_value_expression": "length*diameter^2*(-0.910331972*(depth/diameter)^3+1.376045712*(depth/diameter)^2+0.319684423*(depth/diameter))",
      "ask_user": "false",
      "estimate": "true"
    },
    "ManningCoeff": {
      "type": "value",
      "description": "Manning's Roughness Coefficient",
      "ask_user": "true",
      "delegate": "ValueBox",
      "criteria": "ManningCoeff>0",
      "warningmessage": "Manning's roughness coefficient must be greater than zero",
      "estimate": "true",
      "helptext": "Roughness coefficient representing flow resistance inside the sewer channel. Higher values indicate more resistance."
    },
    "bottom_elevation": {
      "type": "value",
      "description": "Bottom Elevation",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm",
      "estimate": "true",
      "helptext": "Elevation of the sewer pipe bottom relative to the system datum at the center of the sewer pipe. Influences flow direction and energy gradient."
    },
    "diameter": {
      "type": "value",
      "description": "Diameter",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "estimate": "true",
      "criteria": "diameter>0",
      "warningmessage": "Diameter cannot be zero",
      "helptext": "Internal diameter of the sewer channel. Affects storage volume and flow capacity."
    },
    "depth": {
      "type": "expression",
      "expression": "normalized_depth*diameter",
      "description": "Initial Water Depth; Water Depth",
      "ask_user": "true",
      "delegate": "UnitBox",
      "precalcbasedon": "Storage:log:0.000001:1000000",
	  "unit": "m;ft;cm",
      "includeinoutput": "true",
      "criteria": "depth>-0.0000001",
      "warningmessage": "Depth must be non-negative",
      "helptext": "Initial water depth in the sewer segment at the start of simulation."
    },
    "normalized_area": {
      "type": "expression",
      "expression": "Storage/(length*diameter^2)",
      "description": "Normalized area",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "normalized_depth": {
      "type": "expression",
      "expression": "_min(2.2298958*normalized_area^3-2.7110899*normalized_area^2+2.027012721*normalized_area;1)",
      "description": "Normalized water depth",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "head": {
      "type": "expression",
      "expression": "depth+bottom_elevation+_pos(Storage-0.785398163*length*diameter^2)*10000",
      "description": "Hydraulic Head",
      "ask_user": "false",
      "includeinoutput": "true"
    },
    "length": {
      "type": "value",
      "description": "Segment length",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm",
      "estimate": "true",
      "criteria": "length>0",
      "warningmessage": "Length must be greater than zero",
      "helptext": "Length of the sewer channel segment. Affects storage and travel time of flow."
    },
    "xsectionarea": {
      "type": "expression",
      "expression": "diameter^2*(-0.910331972*(depth/diameter)^3+1.376045712*(depth/diameter)^2+0.319684423*(depth/diameter))",
      "description": "Flow cross-sectional area",
      "ask_user": "false",
      "includeinoutput": "true"
    },
    "area": {
      "type": "expression",
      "expression": "length*diameter",
      "description": "Planeview area",
      "ask_user": "false",
      "includeinoutput": "true"
    },
    "perimeter": {
      "type": "expression",
      "expression": "diameter*(5.486583*(depth/diameter)^3-8.60202*(depth/diameter)^2+6.1081605*(depth/diameter))",
      "description": "Wetted Perimeter",
      "ask_user": "false",
      "includeinoutput": "true"
    },
    "inflow": {
      "type": "timeseries",
      "includeinoutput": "true",
      "applylimit": "true",
      "description": "Inflow time series",
      "ask_user": "true",
      "delegate": "Browser",
      "helptext": "Inflow time series (user input required)."
    }
  },
  "Sewer_pipe": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Sewer pipe",
    "icon": {
      "filename": "Sewer_pipe_link.png"
    },
    "diameter": {
      "type": "value",
      "description": "Diameter",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm",
      "estimate": "true",
      "criteria": "diameter>0",
      "warningmessage": "diameter must be positive",
      "helptext": "Diameter (user input required)."
    },
    "ManningCoeff": {
      "type": "value",
      "description": "Manning's roughness coefficient",
      "ask_user": "true",
      "delegate": "ValueBox",
      "unit": "m",
      "estimate": "true",
      "criteria": "ManningCoeff>0",
      "warningmessage": "Manning's roughness coefficient must be greater than zero",
      "helptext": "Manning's roughness coefficient (user input required)."
    },
    "length": {
      "type": "value",
      "description": "Pipe length",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft",
      "estimate": "true",
      "criteria": "length>0",
      "warningmessage": "length must be positive",
      "helptext": "Pipe length (user input required)."
    },
    "start_elevation": {
      "type": "value",
      "description": "Start elevation",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft",
      "helptext": "Start elevation (user input required)."
    },
    "end_elevation": {
      "type": "value",
      "description": "End elevation",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft",
      "helptext": "End elevation (user input required)."
    },
    "hydraulic_gradient": {
      "type": "expression",
      "description": "Hydraulic Gradient",
      "expression": "(head.e-head.s)/length",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "depth_1": {
      "type": "expression",
      "description": "Start Depth",
      "expression": "_min(_pos(head.s-start_elevation);diameter)",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "depth_2": {
      "type": "expression",
      "description": "End Depth",
      "expression": "_min(_pos(head.e-end_elevation);diameter)",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "area_1": {
      "type": "expression",
      "description": "Start area",
      "expression": "diameter^2*(-0.910331972*(depth_1/diameter)^3+1.376045712*(depth_1/diameter)^2+0.319684423*(depth_1/diameter))",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "area_2": {
      "type": "expression",
      "description": "End area",
      "expression": "diameter^2*(-0.910331972*(depth_2/diameter)^3+1.376045712*(depth_2/diameter)^2+0.319684423*(depth_2/diameter))",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "perimeter_1": {
      "type": "expression",
      "description": "Start wetted perimeter",
      "expression": "diameter*(5.486583*(depth_1/diameter)^3-8.60202*(depth_1/diameter)^2+6.1081605*(depth_1/diameter))",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "perimeter_2": {
      "type": "expression",
      "description": "End wetted perimeter",
      "expression": "diameter*(5.486583*(depth_2/diameter)^3-8.60202*(depth_2/diameter)^2+6.1081605*(depth_2/diameter))",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "eff_area": {
      "type": "expression",
      "description": "Effective area",
      "expression": "_hsd(_max(head.s;start_elevation)-_max(head.e;end_elevation))*area_1 + _hsd(_max(head.e;end_elevation)-_max(head.s;start_elevation))*area_2",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "hydraulic_radius": {
      "type": "expression",
      "description": "Effective hydraulic radius",
      "expression": "_hsd(_max(head.s;start_elevation)-_max(head.e;end_elevation))*area_1/perimeter_1 + _hsd(_max(head.e;end_elevation)-_max(head.s;start_elevation))*area_2/perimeter_2",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "flow": {
      "type": "expression",
      "description": "Flow rate (m~^3/day)",
      "expression": "86400*(_sqt((_max(head.s;start_elevation)-_max(head.e;end_elevation))/length)/ManningCoeff)*eff_area*hydraulic_radius^0.6667",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    },
    "color": {
      "type": "string",
      "default": "0,0,200"
    }
  },
  "Catch basin": {
    "type": "block",
    "typecategory": "Blocks",
	"description": "Catch Basin", 
    "icon": {
      "filename": "Catch_basin.png"
    },
    "Storage": {
      "type": "balance",
      "massbalance": "true",
      "flow": "flow",
      "inflow": "inflow",
      "includeinoutput": "true",
      "description": "Initial Storage;Storage",
      "ask_user": "false",
      "default": "0.0001",
      "unit": "m~^3;ft~^3",
      "estimate": "true"
    },
    "inflow": {
      "type": "timeseries",
      "includeinoutput": "true",
      "applylimit": "true",
      "description": "Inflow time series",
      "ask_user": "true",
      "delegate": "Browser",
      "helptext": "Inflow time series (user input required)."
    },
    "area": {
      "type": "value",
      "description": "Base Area",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m~^2;ft~^2",
      "estimate": "true",
      "criteria": "area>0",
      "warningmessage": "Base area must be positive",
      "helptext": "Base Area (user input required)."
    },
    "bottom_elevation": {
      "type": "value",
      "description": "Bottom Elevation",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft",
      "estimate": "true",
      "helptext": "Bottom Elevation (user input required)."
    },
    "depth": {
      "type": "expression",
      "expression": "Storage/area",
      "description": "Water Depth",
      "ask_user": "false",
      "delegate": "UnitBox",
      "unit": "m;ft;cm",
      "includeinoutput": "true"
    },
    "head": {
      "type": "expression",
      "expression": "depth+bottom_elevation",
      "description": "Hydraulic Head",
      "ask_user": "false",
      "includeinoutput": "true"
    }
  },
  "Sewer2Sewer_link": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Link from a sewer pipe block to another",
    "icon": {
      "filename": "sewer2sewer_link.png"
    },
    "area": {
      "type": "expression",
      "description": "Cross-sectional area",
      "expression": "_max(_bkw(head;xsectionarea);0)",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "perimeter": {
      "type": "expression",
      "description": "Wetted Perimeter",
      "expression": "(perimeter.s+perimeter.e)/2",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "hydraulic_radious": {
      "type": "expression",
      "description": "Hydraulic Radius",
      "expression": "area/(perimeter+0.001*diameter.s)",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "ManningCoeff": {
      "type": "expression",
      "description": "Manning's roughness coefficient",
      "expression": "(ManningCoeff.s+ManningCoeff.e)/2",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "length": {
      "type": "expression",
      "description": "Connector length",
      "expression": "(length.s+length.e)/2",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "flow": {
      "type": "expression",
      "description": "Flow (m~^3/day)",
      "expression": "86400*(_sqt((head.s-head.e)/length)/ManningCoeff)*area*hydraulic_radious^0.6667",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    }
  },
  "sewer2fixed": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Link from a sewer pipe segment to fixed head boundary",
    "icon": {
      "filename": "sewer2fixedhead_link.png"
    },
    "area": {
      "type": "expression",
      "description": "Cross-sectional area",
      "expression": "_max(xsectionarea.s;0)",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "perimeter": {
      "type": "expression",
      "description": "Wetted Perimeter",
      "expression": "perimeter.s",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "hydraulic_radious": {
      "type": "expression",
      "description": "Hydraulic Radius",
      "expression": "area/perimeter",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "ManningCoeff": {
      "type": "expression",
      "description": "Manning's roughness coefficient",
      "expression": "ManningCoeff.s",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "length": {
      "type": "expression",
      "description": "Connector length",
      "expression": "length.s",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "flow": {
      "type": "expression",
      "description": "Flow (m~^3/day)",
      "expression": "86400*(_sqt((head.s-head.e)/length)/ManningCoeff)*area*hydraulic_radious^0.6667",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    },
    "Hydraulic Gradient": {
      "type": "expression",
      "description": "Hydralic Gradient",
      "expression": "(head.s-head.e)/length",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    }
  },
  "sewer2pond": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Link from a sewer pipe segment to a pond",
    "icon": {
      "filename": "sewer2pond_link.png"
    },
    "end_elevation": {
      "type": "value",
      "description": "Pipe bottom elevation at the pond",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm",
      "includeinoutput": "false",
      "helptext": "Pipe bottom elevation at the pond (user input required)."
    },
    "depth_2": {
      "type": "expression",
      "description": "End Depth",
      "expression": "_min(_pos(head.e-end_elevation);diameter.s)",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "diameter": {
      "type": "expression",
      "description": "diameter",
      "expression": "diameter.s",
      "includeinoutput": "false",
      "ask_user": "false"
    },
    "area_2": {
      "type": "expression",
      "description": "Downstream area",
      "expression": "diameter^2*(-0.910331972*(depth_2/diameter)^3+1.376045712*(depth_2/diameter)^2+0.319684423*(depth_2/diameter))",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "perimeter_2": {
      "type": "expression",
      "description": "Downstream wetted perimeter",
      "expression": "diameter*(5.486583*(depth_2/diameter)^3-8.60202*(depth_2/diameter)^2+6.1081605*(depth_2/diameter))",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "area_1": {
      "type": "expression",
      "description": "Upstream cross-sectional area",
      "expression": "_max(xsectionarea.s;0)",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "perimeter_1": {
      "type": "expression",
      "description": "Upstream Wetted Perimeter",
      "expression": "perimeter.s",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "hydraulic_radious_1": {
      "type": "expression",
      "description": "Upstream Hydraulic Radius",
      "expression": "area_1/(perimeter_1+0.001*diameter)",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "hydraulic_radious_2": {
      "type": "expression",
      "description": "Downstream Hydraulic Radius",
      "expression": "area_2/(perimeter_2+0.001*diameter)",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "ManningCoeff": {
      "type": "expression",
      "description": "Manning's roughness coefficient",
      "expression": "ManningCoeff.s",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "length": {
      "type": "expression",
      "description": "Connector length",
      "expression": "length.s",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "hydraulic_gradient": {
      "type": "expression",
      "description": "Hydraulic Gradient",
      "expression": "(head.s-head.e)/length",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "flow": {
      "type": "expression",
      "description": "Flow (m~^3/day)",
      "expression": "86400*(_sqt(hydraulic_gradient)/ManningCoeff)*(_hsd(head.s-head.e)*area_1*hydraulic_radious_1^0.6667+_hsd(head.e-head.s)*area_2*hydraulic_radious_2^0.6667)",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    }
  },
  "soil2sewer": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Link from soil to a sewer pipe segment",
    "icon": {
      "filename": "soil2sewer.png"
    },
    "Transmissivity_coefficient": {
      "type": "value",
      "description": "Transmissivity coefficient",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "1/day",
      "estimate": "true",
      "helptext": "Transmissivity coefficient (user input required)."
    },
    "flow": {
      "type": "expression",
      "description": "Flow (m~^3/day)",
      "expression": "Transmissivity_coefficient*(head.s-head.e)*3.1415*diameter.e*length.e",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    }
  },
"Catchment_MRM": {
    "type": "block",
    "typecategory": "Blocks",
    "description": "Catchment (MRM)",
    "icon": {
      "filename": "Catchment_MRM.png"
    },
    "Storage": {
      "type": "balance",
      "massbalance": "true",
      "flow": "flow",
      "inflow": "Precipitation,Precipitation_loss,Evapotranspiration,loss,inflow",
      "includeinoutput": "true",
      "description": "Initial Storage;Storage",
      "ask_user": "false",
      "initial_value_expression": "area*depth",
      "estimate": "true"
    },
    "inflow": {
      "type": "timeseries",
      "includeinoutput": "true",
      "description": "Inflow",
      "ask_user": "true",
      "delegate": "Browser",
      "helptext": "User-supplied inflow time series to the catchment. This could represent runoff, pipe inflow, or any other external water source."
    },
    "Precipitation": {
      "type": "source",
      "includeinoutput": "true",
      "description": "Precipitation",
      "helptext": "A precipitation source object must be selected.",
      "ask_user": "true",
      "delegate": "ComboBox;Sources"
    },
    "Precipitation_runoff": {
      "type": "expression",
      "expression": "Precipitation*Runoff_coeff",
      "includeinoutput": "true",
      "description": "Effective precipitation",
      "helptext": "Precipitation multiplied by runoff coefficient",
      "ask_user": "false"
    },
    "Precipitation_loss": {
      "type": "expression",
      "expression": "-Precipitation*(1-Runoff_coeff)",
      "includeinoutput": "true",
      "description": "Precipitation loss",
      "helptext": "Precipitation loss",
      "ask_user": "false"
    },
    "Evapotranspiration": {
      "type": "source",
      "includeinoutput": "true",
      "description": "Evapotranspiration",
      "helptext": "A evapotranspiration 'source' object must be selected. In order to add an evapotransporation model a plug-in containing evapotranspiration models first needed to be added to the model",
      "ask_user": "true",
      "delegate": "ComboBox;Sources"
    },
    "loss": {
      "type": "expression",
      "expression": "-loss_coefficient*Storage",
      "includeinoutput": "true",
      "description": "Loss rate",
      "ask_user": "false"
    },
    "area": {
      "type": "value",
      "description": "Catchment area",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m~^2;ft~^2",
      "estimate": "true",
      "criteria": "area>0",
      "warningmessage": "Area must be greater than zero",
      "helptext": "Total horizontal surface area of the catchment. Used to compute initial storage and flow-related parameters."
    },
    "Slope": {
      "type": "value",
      "description": "Catchment slope in the direction of flow",
      "helptext": "The average slope of the catchment along the dominant flow direction",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true"
    },
    "Runoff_coeff": {
      "type": "value",
      "description": "Runoff coefficient",
      "helptext": "This is the fraction of rain that converts to runoff in case infiltration is not explicitly modeled",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true",
      "default": "1"
    },
    "length": {
      "type": "value",
      "description": "Flow path length",
      "helptext": "Maximum flow path length",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "estimate": "true"
    },
    "depth": {
      "type": "expression",
      "description": "Average water depth",
      "expression": "Storage/area",
      "helptext": "The initial depth of the standing water before the simulation start time. Most of the times, this value is zero",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "ask_user": "true",
      "includeinoutput": "true"
    },
    "elevation": {
      "type": "value",
      "description": "Mean elevation of the catchment",
      "helptext": "The elevation of the centroid of the catchment based on the model datum.",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft"
    },
    "head": {
      "type": "expression",
      "expression": "elevation+depth",
      "description": "Average water head",
      "ask_user": "false",
      "includeinoutput": "true"
    },
    "depression_storage": {
      "type": "value",
      "description": "Depression storage",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "helptext": "The depth of water that is retained in surface depressions and does not contribute to surface runoff."
    },
    "loss_coefficient": {
      "type": "value",
      "default": "0",
