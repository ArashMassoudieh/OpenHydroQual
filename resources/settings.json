{
  "solver_settings": {
    "description": "Solver Settings",
    "type": "setting",
    "typecategory": "Settings",
      "c_n_weight": {
      "type": "value",
      "default": "1",
      "description": "Crank-Nicholson Time Weight",
      "ask_user": "true",
      "delegate": "ValueBox",
      "code": "cn_weight"
    },
    "name": {
      "description": "Name",
      "setvalue": "Solver Settings",
      "type": "string",
      "ask_user": "false",
      "delegate": "String" 
    },
    "n_threads": {
      "type": "value",
      "default": "4",
      "description": "Number of threads to be used",
      "ask_user": "true",
      "delegate": "ValueBox",
      "code": "n_threads"
    },
    "nr_tolerance": {
      "type": "value",
      "default": "0.001",
      "description": "Newton-Raphson Tolerance",
      "ask_user": "true",
      "delegate": "ValueBox",
      "code": "nr_tolerance"
    },
    "nr_timestep_reduction_factor": {
      "type": "value",
      "default": "0.75",
      "description": "Newton-Raphson Time-step reduction factor",
      "ask_user": "true",
      "delegate": "ValueBox",
      "code": "nr_timestep_reduction_factor"
    },
    "nr_timestep_reduction_factor_fail": {
      "type": "value",
      "default": "0.2",
      "description": "Newton-Raphson Time-step reduction factor when fail",
      "ask_user": "true",
      "delegate": "ValueBox",
      "code": "nr_timestep_reduction_factor_fail"
    },
    "minimum_timestep": {
      "type": "value",
      "default": "1e-6",
      "description": "Minimum time-step",
      "ask_user": "true",
      "delegate": "ValueBox",
      "code": "minimum_timestep"
    },
      "initial_time_step": {
      "type": "value",
      "default": "1e-2",
      "description": "Initial time-step",
      "ask_user": "true",
      "delegate": "ValueBox",
      "code": "dt"
    },
      "write_solution_details": {
      "type": "string",
      "default": "No",
      "description": "Write Solution Details",
      "ask_user": "true",
      "delegate": "ComboBox, items: Yes, No",
      "code": "write_solution_details"
    }
  },
  "general_parameters": {
    "type": "setting",
    "typecategory": "Settings",
    "name": {
      "description": "Name",
      "setvalue": "General Settings",      
      "type": "string",
      "ask_user": "false",
      "delegate": "String" 
    },
    "description": "General Parameters",
    "simulation_start_time": {
      "default": "0",
      "description": "Simulation start time",
      "ask_user": "true",
      "type": "value",
      "delegate": "DateTime",
      "code": "time_min",
      "criteria": "simulation_start_time<simulation_end_time",
      "warningmessage": "Start time must be smaller than end time"
    },
    "simulation_end_time": {
      "default": "1",
      "description": "Simulation end time",
      "ask_user": "true",
      "type": "value",
      "delegate": "DateTime",
      "code": "time_max",
      "criteria": "simulation_start_time<simulation_end_time",
      "warningmessage": "End time must be greater than start time"
    }
  },
  "optimizer": {
    "type": "setting",
    "typecategory": "Settings",
    "name": {
      "description": "Name",
      "setvalue": "Optimizer",      
      "type": "string",
      "ask_user": "false",
      "delegate": "String" 
    },
    "description": "Optimizer Parameters",
    "maxpop": {
      "default": "40",
      "description": "Genetic Algorithm Population",
      "ask_user": "true",
      "type": "value",
      "delegate": "ValueBox",
      "code": "maxpop"
    },
    "ngen": {
      "default": "40",
      "description": "Number of generations",
      "ask_user": "true",
      "type": "value",
      "delegate": "ValueBox",
      "code": "ngen"
    },
    "pcross": {
      "default": "1",
      "description": "Cross over probability",
      "ask_user": "true",
      "type": "value",
      "delegate": "ValueBox",
      "code": "pcross"
    },
    "pmute": {
      "default": "0.02",
      "description": "Mutation Probability",
      "ask_user": "true",
      "type": "value",
      "delegate": "ValueBox",
      "code": "pmute"
    },
    "shakescale": {
      "default": "0.05",
      "description": "Shaking Scale",
      "ask_user": "true",
      "type": "value",
      "delegate": "ValueBox",
      "code": "shakescale"
    },
    "shakescalered": {
      "default": "0.75",
      "description": "Shake Scale Reduction Factor",
      "ask_user": "true",
      "type": "value",
      "delegate": "ValueBox",
      "code": "shakescalered"
    },
    "outputfile": {
      "default": "GA_output.txt",
      "description": "GA Output File Name",
      "ask_user": "true",
      "type": "string",
      "delegate": "Browser",
      "code": "outputfile"
    }
  }
}
