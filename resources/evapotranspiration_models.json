{
  "Evapotranspiration_Aerodynamic": {
    "type": "source",
    "typecategory": "Sources",
    "icon": {
      "filename": "Evapotranspiration_Aerodynamic.png"
    },
    "name": {
      "description": "Name",      
      "type": "string",
      "ask_user": "true",
      "delegate": "String"
    },
    "coefficient": {
      "type": "expression",
      "expression": "area",
      "ask_user": "false",
      "delegate": "UnitBox"
    },
    "rate": {
      "type": "expression",
      "includeinoutput": "true",
      "description": "Evapotranspiration rate",
      "ask_user": "false",
      "expression": "B*e_as*(R_h-1)"
    },
    "wind_speed":{
    	"type": "timeseries",
	"includeinoutput": "true",
	"description": "wind time series",
    	"ask_user": "true",
	"delegate": "Browser"
    },
    "Temperature":{
    	"type": "timeseries",
	"includeinoutput": "true",
	"description": "Temperature time series",
    	"ask_user": "true",
	"delegate": "Browser"
    },
    "B": {
    	"type": "expression",
	"includeinoutput": "false",
	"description": "B coefficient",
	"ask_user": "false",
	"expression": "0.102*wind_speed/(_log(z2/z0))^2"
    },
    "z2" : {
    	"type": "value",
    	"ask_user": "true",
	"includeinoutput": "false",
    	"description": "Wind measurement elevation",
    	"default": "2",
    	"criteria": "z2>0",
        "warningmessage": "Wind reference height must be greater than zero",
    	"delegate": "UnitBox"
    },
    "z0" : {
    	"type": "value",
    	"ask_user": "true",
	"includeinoutput": "false",
    	"description": "Roughness height",
    	"default": "0.05",
    	"criteria": "z0>0",
       "warningmessage": "Roughness height must be greater than zero",
    	"delegate": "UnitBox"
    },
    "e_as": {
    	"type": "expression",
    	"expression": "611*_exp(17.27*Temperature/(237.3+Temperature))",
	"includeinoutput": "true",
	"description": "Saturated humidity"
    },
    "R_h" : {
    	"type": "timeseries",
    	"includeinoutput": "true",
	"description": "Relative humidity timeseries",
    	"ask_user": "true",
	"delegate": "Browser"
    }
  }
}
