{
  "Unconfined Groundwater cell": {
    "type": "block",
    "typecategory": "Blocks",
    "description": "Unconfined Aquifer Cell",
    "icon": {
      "filename": "unconfined_aquifer.png"
    },
    "Storage": {
      "type": "balance",
      "massbalance": "true",
      "flow": "flow",
      "inflow": "recharge,Precipitation",
      "includeinoutput": "true",
      "description": "Initial Storage;Storage",
      "initial_value_expression": "area*(piezometric_head-base_elevation)*specific_yield",
      "ask_user": "false",
      "estimate": "true"
    },
    "Precipitation": {
      "type": "source",
      "includeinoutput": "true",
      "description": "precipitation time series",
      "ask_user": "true",
      "delegate": "ComboBox;Sources"
    },
    "recharge": {
      "type": "expression",
      "expression": "recharge_per_area*area",      
      "description": "Total recharge",
      "ask_user": "false",
      "includeinoutput": "true"
    },
    "recharge_per_area": {
      "type": "timeseries",
      "includeinoutput": "true",
      "description": "Recharge per unit area [m/day]",
      "ask_user": "true",
      "delegate": "Browser"
    },
    "hydraulic_conductivity": {
      "type": "value",
      "description": "Hydraulic Conductivity",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m/day;ft/day;cm/s;in/s;m/s",
      "estimate": "true",
      "default": "1",
      "criteria": "hydraulic_conductivity>0",
      "warningmessage": "Hydraulic conductivity must be greater than zero"
    },
    "K_eff": {
      "type": "expression",
      "expression": "hydraulic_conductivity",      
      "description": "Hydraulic conductivity",
      "ask_user": "false",
      "includeinoutput": "false"
    },
    "porosity": {
      "type": "value",
      "description": "Porosity",
      "ask_user": "true",
      "default": "0.4",
      "delegate": "ValueBox",
      "estimate": "true"
    },
    "specific_yield": {
      "type": "value",
      "description": "Specific Yield",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true",
      "default": "0.3",
      "criteria": "specific_yield>0",
      "warningmessage": "Specific Yield must be greater than zero"
    },
    "base_elevation": {
      "type": "value",
      "description": "Base Elevation",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "estimate": "true"
    },
    "piezometric_head": {
      "type": "expression",
      "expression": "Storage/(specific_yield*area)+base_elevation",      
      "description": "Piezometric head",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "includeinoutput": "true"
    },
    "depth": {
      "type": "expression",
      "expression": "piezometric_head-base_elevation",      
      "description": "Water depth",
      "ask_user": "false",
      "includeinoutput": "true"
    },
    "area": {
      "type": "value",
      "description": "Cell area",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m~^2;ft~^2;cm~^2;in~^2",
      "includeinoutput": "false",
      "criteria": "area>0",
      "warningmessage": "Cell area must be greater than zero"
    }
  },
  "UC_groundwater_link": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Unconfined Aquifer link",
    "icon": {
      "filename": "unconfined_GW2unconfined_GW.png"
    },
    "length": {
      "type": "value",
      "description": "Centroid to centroid length",
      "includeinoutput": "false",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "criteria": "length>0",
      "warningmessage": "Length must be greater than zero"
    },
    "width": {
      "type": "value",
      "description": "Width of the connector",
      "includeinoutput": "false",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "criteria": "width>0",
      "warningmessage": "Width must be greater than zero"
    },
    "hydraulic_gradient": {
      "type": "expression",
      "description": "Hydraulic Gradient",
      "expression": "(piezometric_head.s-piezometric_head.e)/length",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "flow": {
      "type": "expression",
      "description": "Flow rate",
      "expression": "0.25*(hydraulic_conductivity.s+hydraulic_conductivity.e)*(piezometric_head.s-piezometric_head.e)*width*(depth.s+depth.e)/length",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    },
    "Darcy flux": {
      "type": "expression",
      "description": "Darcy flux",
      "expression": "0.5*(hydraulic_conductivity.s+hydraulic_conductivity.e)*(piezometric_head.s-piezometric_head.e)/length",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    }
  },
  "unconfined_groundwater_to_fixedhead": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Unconfined Groundwater to fixed head",
    "icon": {
      "filename": "unconfined_GW2fixedhead.png"
    },
    "length": {
      "type": "value",
      "description": "Length",
      "includeinoutput": "false",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "criteria": "length>0",
      "warningmessage": "Length must be greater than zero"
    },
    "width": {
      "type": "value",
      "description": "Interface Width",
      "includeinoutput": "false",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "criteria": "width>0",
      "warningmessage": "Width must be greater than zero"
    },
    "hydraulic_gradient": {
      "type": "expression",
      "description": "Hydraulic Gradient",
      "expression": "(piezometric_head.s-head.e)/length",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "flow": {
      "type": "expression",
      "description": "Flow rate",
      "expression": "hydraulic_conductivity.s*(piezometric_head.s-head.e)*width*depth.s/length",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    },
    "Darcy flux": {
      "type": "expression",
      "description": "Darcy flux",
      "expression": "hydraulic_conductivity.s*(piezometric_head.s-head.e)/length",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    }
  },
  "surface2UCgroundwater_link": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Surface water to unconfined groundwater link",
    "icon": {
      "filename": "Surface2GW_link.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "length": {
      "type": "value",
      "description": "Length",
      "includeinoutput": "false",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "criteria": "length>0",
      "warningmessage": "Length must be greater than zero"
    },
    "area": {
      "type": "value",
      "description": "Interface Area",
      "includeinoutput": "false",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m~^2;ft~^2;cm~^2;in~^2",
      "criteria": "area>0",
      "warningmessage": "Interface area must be greater than zero"
    },
    "hydraulic_conductivity": {
      "type": "value",
      "description": "Hydraulic conductivity",
      "includeinoutput": "false",
      "ask_user": "true",
      "delegate": "UnitBox",
    	"unit": "m/day;ft/day;cm/s;in/s;m/s",
      "criteria": "hydraulic_conductivity>0",
      "warningmessage": "Hydraulic conductivity must be greater than zero"
    },
    "flow": {
      "type": "expression",
      "description": "Flow",
      "expression": "area*hydraulic_conductivity*(head.s-head.e)/length",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    }
  },
  "water_supply_well": {
    "type": "block",
    "typecategory": "Blocks",
    "icon": {
      "filename": "water_supply_well.png"
    },
    "Storage": {
      "type": "balance",
      "massbalance": "true",
      "flow": "flow",
      "includeinoutput": "false",
      "description": "Storage"
    },
    "pumping_rate": {
      "type": "timeseries",
      "includeinoutput": "true",
      "description": "Pumping rate time series [m~^3/d]",
      "ask_user": "true",
      "delegate": "Browser"
    },
    "bottom_elevation": {
      "type": "value",
      "description": "The bottom elevation of the well",
      "includeinoutput": "false",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft"
    }
  },
  "aquifer2well_link": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Unconfined to Well connector",
    "icon": {
      "filename": "unconfined_aquifer2well.png"
    },
    "flow": {
      "type": "expression",
      "description": "Flow",
      "expression": "pumping_rate.e*_pos(piezometric_head.s-bottom_elevation.e)/(0.01+_pos(piezometric_head.s-bottom_elevation.e))",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    }
  }
}
