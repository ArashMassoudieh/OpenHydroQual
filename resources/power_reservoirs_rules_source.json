      "ask_user": "true",
      "delegate": "ComboBox;Sources"
    },
    "Area": {
      "type": "value",
      "description": "Catchment area",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m~^2",
      "estimate": "true",
	   "criteria": "Area>0",
	  "warningmessage": "Area must be greater than zero"
    },
    "x": {
      "type": "value",
      "description": "x",
      "ask_user": "true",
      "delegate": "UnitBox"
    },
    "y": {
      "type": "value",
      "description": "y",
      "ask_user": "true",
      "delegate": "UnitBox"
    },
