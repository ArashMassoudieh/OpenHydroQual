{
  "Storage Unit": {
    "type": "block",
    "typecategory": "Blocks",
    "description": "Storage Unit",
    "icon": {
      "filename": "StorageUnit.png"
    },
    "Storage": {
      "type": "balance",
      "massbalance": "true",
      "flow": "flow",
      "inflow":"PrecipitationAdjusted",
      "includeinoutput": "true",
      "description": "Initial Storage; Storage",
      "ask_user": "true",
      "estimate": "true"
    },
    "Capacity": {
       "type": "value",
       "description": "Capacity",      
       "ask_user": "true",
       "includeinoutput": "true",
	   "delegate": "UnitBox",
	   "unit": "m~^3;ft~^3",
	   "estimate": "true",
	   "criteria": "Capacity>0",
       "warningmessage": "Capacity must be greater than zero"
    },
    "Precipitation": {
      "type": "source",
      "includeinoutput": "true",
      "description": "Precipitation",
      "helptext": "A precipitation source object must be selected.",  
      "ask_user": "true",
      "delegate": "ComboBox;Sources"
    },
    "Shift_factor": {
      "type": "value",
      "description": "Hydraulic Conductivity",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true",
      "default": "1"
    },
	"Scale_factor": {
      "type": "value",
      "description": "Scale factor",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true",
      "default": "1",
	  "criteria": "Scale_factor>0",
      "warningmessage": "Scale factor must be greater than zero"
    },
	"Latent_variable": {
	  "type": "expression",
      "description": "Latent variable",
	  "expression": "Scale_factor*_log(Shift_factor*Storage/(Capacity-Storage))",
	  "includeinoutput": "true",
	  "ask_user": "false"
	}
  },
  "storage_unit_link": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Storage Unit Link",
    "icon": {
      "filename": "StorageUnitLink.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "Mean_Transmissivity": {
		"type": "value",
		"description": "Mean Transmissivity",
		"ask_user": "true",
		"delegate": "UnitBox",
		"estimate": "true",
		"unit": "m~^2/day; ft~^2/day",
		"default": "1"
	},
	"Smoothness_Factor": {
		"type": "value",
		"description": "Smoothness Factor",
		"ask_user": "true",
		"delegate": "UnitBox",
		"estimate": "true",
		"unit": "m~^3/day; ft~^3/day",
		"default": "1"
	},
	"Nonlinearity_Factor": {
		"type": "value",
		"description": "Nonlinearity Factor",
		"ask_user": "true",
		"delegate": "UnitBox",
		"estimate": "true",
		"unit": "m~^1; ft~^1",
		"default": "1"
	},
	"ExpTerm": {
      "type": "expression",
      "description": "Exponential Term",
      "expression": "_exp(-Nonlinearity_Factor*(Latent_variable.s - Latent_variable.e))",
      "includeinoutput": "true",
      "ask_user": "false"
    },
	"flow": {
      "type": "expression",
      "description": "Flow rate",
      "expression": "Mean_Transmissivity*(Latent_variable.s-Latent_variable.e) + Smoothness_Factor/2*(1-ExpTerm)/(1+ExpTerm)",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    }
  }
}