{
  "solutionorder": [ "Storage" ],
  "Catchment": {
    "type": "block",
    "typecategory": "Blocks",
    "icon": {
      "filename": "rainwater-catchment.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String"
    },
    "Storage": {
      "type": "balance",
      "massbalance": "true",
      "flow": "flow",
      "inflow": "Precipitation,Evapotranspiration",
      "includeinoutput": "true",
      "description": "Initial Storage",
      "ask_user": "false",
      "initial_value_expression": "area*depth",
      "estimate": "true"
    },
    "Precipitation": {
      "type": "source",
      "includeinoutput": "true",
      "description": "precipitation time series",
      "ask_user": "true",
      "delegate": "ComboBox;Sources"
    },
    "Evapotranspiration": {
      "type": "source",
      "includeinoutput": "true",
      "description": "Evapotranspiration",
      "ask_user": "true",
      "delegate": "ComboBox;Sources"
    },
    "area": {
      "type": "value",
      "description": "Catchment area",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m~^2;ft~^2",
      "estimate": "true",
      "criteria": "area>0",
      "warningmessage": "Area must be greater than zero"
    },
    "Slope": {
      "type": "value",
      "description": "Catchment slope in the direction of flow",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true"
    },
    "ManningCoeff": {
      "type": "value",
      "description": "Manning's Roughness Coefficient",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true",
      "criteria": "ManningCoeff>0",
      "warningmessage": "Manning's roughness coefficient must be greater than zero"
    },
    "Width": {
      "type": "value",
      "description": "The width of the catchment",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "estimate": "true"
    },
    "depth": {
      "type": "expression",
      "description": "Water depth",	
      "expression": "Storage/area",
      "description": "Average water depth",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "ask_user": "true",
      "includeinoutput": "true"
    },
    "elevation": {
      "type": "value",
      "description": "Mean elevation of the catchment",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft"
    },
    "head": {
      "type": "expression",
      "expression": "elevation+depth",
      "description": "Average water head",
      "ask_user": "false",
      "includeinoutput": "true"
    }
  },
  "Precipitation": {
    "type": "source",
    "typecategory": "Sources",
    "icon": {
      "filename": "cloud.png"
    },
    "name": {
      "description": "Name",      
      "type": "string",
      "ask_user": "true",
      "delegate": "String"
    },
    "coefficient": {
      "type": "expression",
      "expression": "area",
      "ask_user": "false",
      "delegate": "ValueBox"
    },
    "timeseries": {
      "type": "timeseries_prec",
      "includeinoutput": "true",
      "description": "Precipitation intensity",
      "ask_user": "true",
      "delegate": "Browser;time series"
    }
  },
  "Reservoir": {
    "type": "block",
    "typecategory": "Blocks",
    "icon": {
      "filename": "Reservoir_Rating_Curve.png"
    },
    "Storage": {
      "type": "balance",
      "massbalance": "true",
      "flow": "flow",
      "inflow": "inflow",
      "includeinoutput": "true",
      "description": "Initial Storage",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m~^3;ft~^3",
      "estimate": "true"
    },
    "reservoir_power": {
      "type": "value",
      "default": "2",
      "description": "Reservoir rating curve power",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true"
    },
    "reservoir_coeff": {
      "type": "value",
      "default": "10",
      "description": "Reservoir rating curve coefficient",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true"
    },
    "inflow": {
      "type": "timeseries",
      "includeinoutput": "true",
      "applylimit": "true",
      "description": "Flow rate time series",
      "ask_user": "true",
      "delegate": "Browser"
    }
  },
  "Special_Reservoir": {
    "type": "block",
    "typecategory": "Blocks",
    "icon": {
      "filename": "Reservoid_Control.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String"
    },
    "Storage": {
      "type": "balance",
      "massbalance": "true",
      "flow": "flow",
      "inflow": "inflow",
      "includeinoutput": "true",
      "description": "Initial Storage",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m~^3;ft~^3",
      "estimate": "true"
    },
    "reservoir_power": {
      "type": "value",
      "default": "2",
      "description": "Reservoir rating curve power",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true"
    },
    "reservoir_coeff": {
      "type": "value",
      "default": "10",
      "description": "Reservoir rating curve coefficient",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true"
    },
    "inflow": {
      "type": "timeseries",
      "includeinoutput": "true",
      "applylimit": "true",
      "description": "Flow rate time series",
      "ask_user": "true",
      "delegate": "Browser"
    }
  },
  "Reservoir_link": {
    "type": "link",
    "typecategory": "Connectors",
    "icon": {
      "filename": "Reservoir_Link_Arrow.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "flow": {
      "type": "expression",
      "expression": "reservoir_coeff.s*Storage.s^reservoir_power.s",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    }
  },
  "Catchment_link": {
    "type": "link",
    "typecategory": "Connectors",
    "icon": {
      "filename": "Catchment_outlet.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "flow": {
      "type": "expression",
      "expression": "86400*(Width.s/ManningCoeff.s)*(Slope.s^0.5)*(_pos(depth.s)^1.666667)",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    },
    "color": {
      "type": "string",
      "description": "color",
      "default": "200,0,100",
      "ask_user": "false"
    }
  },
  "Reservoir_link_rule": {
    "type": "link",
    "typecategory": "Connectors",
    "icon": {
      "filename": "reservoir_link.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "flow": {
      "type": "rule",
      "rule": {
        "Storage.s<S_min": "Q_min",
        "S_min<Storage.s<S_max": "Q_min+((Storage.s-S_min)/(S_max-S_min))*(Q_max-Q_min)",
        "Storage.s>S_max": "Q_max"
      },
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false",
      "description": "Flow"
    },
    "Q_min": {
      "type": "value",
      "default": "10",
      "description": "Minimum flow",
      "delegate": "UnitBox",
      "unit": "m~^3/day;ft~^3/day",
      "ask_user": "true",
      "estimate": "true"
    },
    "Q_max": {
      "type": "value",
      "default": "100",
      "description": "Maximum flow",
      "delegate": "UnitBox",
      "unit": "m~^3/day;ft~^3/day",
      "ask_user": "true",
      "estimate": "true"
    },
    "S_min": {
      "type": "value",
      "default": "10",
      "description": "Minimum storage",
      "delegate": "UnitBox",
      "unit": "m~^3;ft~^3",
      "ask_user": "true",
      "estimate": "true"
    },
    "S_max": {
      "type": "value",
      "default": "100",
      "description": "Maximum storage",
      "delegate": "UnitBox",
      "unit": "m~^3;ft~^3",
      "ask_user": "true",
      "estimate": "true"
    }
  },
