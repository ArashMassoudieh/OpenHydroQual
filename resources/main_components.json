{
  "solutionorder": [ "Storage" ],
  "Catchment": {
    "type": "block",
    "typecategory": "Blocks",
    "description": "Catchment",
    "icon": {
      "filename": "rainwater-catchment.png"
    },
    "Storage": {
      "type": "balance",
      "massbalance": "true",
      "flow": "flow",
      "inflow": "Precipitation,Evapotranspiration,loss,inflow_timeseries",
      "includeinoutput": "true",
      "description": "Initial Storage;Storage",
      "ask_user": "false",
      "initial_value_expression": "area*depth",
      "estimate": "true"
    },
    "inflow_timeseries":{
    	"type": "timeseries",
	"includeinoutput": "true",
	"description": "Inflow",
    	"ask_user": "true",
	"delegate": "Browser"
    },
    "Precipitation": {
      "type": "source",
      "includeinoutput": "true",
      "description": "precipitation time series",
      "helptext": "A precipitation source object must be selected.",  
      "ask_user": "true",
      "delegate": "ComboBox;Sources"
    },
    "Evapotranspiration": {
      "type": "source",
      "includeinoutput": "true",
      "description": "Evapotranspiration",
      "helptext": "A evapotranspiration 'source' object must be selected. In order to add an evapotransporation model a plug-in containing evapotranspiration models first needed to be added to the model",
      "ask_user": "true",
      "delegate": "ComboBox;Sources"
    },
    "loss": {
      "type": "expression",
      "expression": "-loss_coefficient*Storage",
      "includeinoutput": "true",
      "description": "Loss rate",
      "ask_user": "false",
      "delegate": "ComboBox;Sources"
    },
    "area": {
      "type": "value",
      "description": "Catchment area",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m~^2;ft~^2",
      "estimate": "true",
      "criteria": "area>0",
      "warningmessage": "Area must be greater than zero"
    },
    "Slope": {
      "type": "value",
      "description": "Catchment slope in the direction of flow",
      "helptext": "The average slope of the catchment along the dominant flow direction",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true"
    },
    "ManningCoeff": {
      "type": "value",
      "description": "Manning's Roughness Coefficient",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true",
      "criteria": "ManningCoeff>0",
      "warningmessage": "Manning's roughness coefficient must be greater than zero"
    },
    "Width": {
      "type": "value",
      "description": "The width of the catchment",
      "helptext": "The average width of the catchment perpendicular to the dominant flow direction",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "estimate": "true"
    },
    "depth": {
      "type": "expression",
      "description": "Initial water depth; Water depth",	
      "expression": "Storage/area",
      "description": "Average water depth",
      "helptext": "The initial depth of the standing water before the simulation start time. Most of the times, this value is zero",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in",
      "ask_user": "true",
      "includeinoutput": "true"
    },
    "elevation": {
      "type": "value",
      "description": "Mean elevation of the catchment",
      "helptext": "The elevation of the centroid of the catchment based on the model datum.",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft"
    },
    "head": {
      "type": "expression",
      "expression": "elevation+depth",
      "description": "Average water head",
      "ask_user": "false",
      "includeinoutput": "true"
    },
    "depression_storage": {
      "type": "value",
      "description": "Depression storage",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm;in"
    },
    "loss_coefficient": {
      "type": "value",
      "default": "0", 
      "description": "Loss coefficient",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "1/day;1/hr"
    }
  },
  "Precipitation": {
    "type": "source",
    "typecategory": "Sources",
    "description": "Precipitation",
    "icon": {
      "filename": "cloud.png"
    },
    "name": {
      "description": "Name",      
      "type": "string",
      "ask_user": "true",
      "delegate": "String"
    },
    "coefficient": {
      "type": "expression",
      "expression": "area",
      "ask_user": "false",
      "delegate": "ValueBox"
    },
    "timeseries": {
      "type": "timeseries_prec",
      "includeinoutput": "true",
      "description": "Precipitation intensity",
      "ask_user": "true",
      "delegate": "Browser;time series"
    }
  },
  "Catchment_link": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Link from a catchment",
    "icon": {
      "filename": "Catchment_outlet.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "flow": {
      "type": "expression",
      "description" : "Flow",
      "expression": "86400*(Width.s/ManningCoeff.s)*(Slope.s^0.5)*(_pos(depth.s-depression_storage.s)^1.666667)",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    },
    "color": {
      "type": "string",
      "description": "color",
      "default": "200,0,100",
      "ask_user": "false"
    }
  },
  "Reservoir_link_rule": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Reservoir outflow with a rule-based flow",
    "icon": {
      "filename": "reservoir_link.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "flow": {
      "type": "rule",
      "desctiption" : "Flow",
      "rule": {
        "Storage.s<S_min": "Q_min",
        "S_min<Storage.s<S_max": "Q_min+((Storage.s-S_min)/(S_max-S_min))*(Q_max-Q_min)",
        "Storage.s>S_max": "Q_max"
      },
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false",
      "description": "Flow"
    },
    "Q_min": {
      "type": "value",
      "default": "10",
      "description": "Minimum flow",
      "delegate": "UnitBox",
      "unit": "m~^3/day;ft~^3/day",
      "ask_user": "true",
      "estimate": "true"
    },
    "Q_max": {
      "type": "value",
      "default": "100",
      "description": "Maximum flow",
      "delegate": "UnitBox",
      "unit": "m~^3/day;ft~^3/day",
      "ask_user": "true",
      "estimate": "true"
    },
    "S_min": {
      "type": "value",
      "default": "10",
      "description": "Minimum storage",
      "delegate": "UnitBox",
      "unit": "m~^3;ft~^3",
      "ask_user": "true",
      "estimate": "true"
    },
    "S_max": {
      "type": "value",
      "default": "100",
      "description": "Maximum storage",
      "delegate": "UnitBox",
      "unit": "m~^3;ft~^3",
      "ask_user": "true",
      "estimate": "true"
    }
  },
  "User": {
    "type": "block",
    "typecategory": "Blocks",
    "description": "Water user",
    "icon": {
      "filename": "farm.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String"
    },
    "Storage": {
      "type": "balance",
      "massbalance": "true",
      "flow": "flow",
      "includeinoutput": "true",
      "description": "Initial Storage;Storage",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m~^3;ft~^3",
      "estimate": "true"
    }
  },
  "User_flow": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Flow link to water user",
    "icon": {
      "filename": "farm_link.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "flow": {
      "type": "timeseries",
      "includeinoutput": "true",
      "description": "Flow",
      "applylimit": "true",
      "description": "Flow rate time series",
      "ask_user": "true",
      "delegate": "Browser" 
    }
  },
  "Parameter": {
    "type": "parameter",
    "typecategory": "Parameters",
    "description": "Parameter",
    "icon": {
      "filename": "parameter.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "low": {
      "type": "value",
      "includeinoutput": "true",
      "description": "Lower Limit",
      "ask_user": "true",
      "delegate": "ValueBox",
      "criteria": "low<high",
      "warningmessage": "Upper Limit must be greater than lower limit" 
    },
    "high": {
      "type": "value",
      "includeinoutput": "true",
      "description": "Upper Limit",
      "ask_user": "true",
      "delegate": "ValueBox",
      "criteria": "high>low",
      "warningmessage": "Upper Limit must be greater than lower limit" 
    },
    "value": {
      "type": "value",
      "includeinoutput": "true",
      "description": "Value",
      "ask_user": "true",
      "delegate": "ValueBox" 
    },
    "prior_distribution": {
      "type": "string",
      "delegate": "ComboBox;items:normal,log-normal",
      "description": "Prior distribution",
      "default": "normal",
      "includeinoutput": "false",
      "ask_user": "true"
    }
  },
  "Objective_Function": {
    "type": "objective_function",
    "description": "Objective function",
    "typecategory": "Objective Functions",
    "icon": {
      "filename": "objective.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "object": {
      "delegate": "ComboBox;BlockLinks",
      "type": "string",
      "includeinoutput": "false",
      "description": "Object",
      "ask_user": "true"
    },
    "method": {
      "type": "string",
      "delegate": "ComboBox;items:integrate,value,maximum,variance,exceedance",
      "includeinoutput": "false",
      "description": "Calculation method",
      "ask_user": "true"
    },
    "expression": {
      "delegate": "expressionEditor",
      "type": "expression",
      "includeinoutput": "true",
      "description": "Expression",
      "ask_user": "true"
    },
    "weight": {
      "type": "value",
      "includeinoutput": "false",
      "description": "Weight",
      "ask_user": "true",
      "delegate": "ValueBox" 
    },
    "exceedance_probability": {
      "type": "value",
      "includeinoutput": "false",
      "description": "Exceedance probability",
      "ask_user": "true",
      "delegate": "ValueBox",
      "default": 0.05 
    }
  },
  "Observation": {
    "type": "observation",
    "description": "Observation",
    "typecategory": "Observations",
    "icon": {
      "filename": "observation.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "object": {
      "delegate": "ComboBox;BlockLinks",
      "type": "string",
      "includeinoutput": "false",
      "description": "Object",
      "ask_user": "true"
    },
    "expression": {
      "delegate": "expressionEditor",
      "type": "expression",
      "includeinoutput": "true",
      "description": "Expression",
      "ask_user": "true"
    },
    "observed_data": {
      "type": "timeseries",
      "includeinoutput": "true",
      "description": "Observed data time series",
      "ask_user": "true",
      "delegate": "Browser"
    },
    "error_structure": {
      "type": "string",
      "delegate": "ComboBox;items:normal,log-normal",
      "description": "Error probability distribution",
      "default": "normal",
      "includeinoutput": "false",
      "ask_user": "true"
    },
    "error_standard_deviation": {
      "type": "value",
      "description": "Error standard deviation",
      "default": "1",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true"
    }
  },
  "fixed_head": {
    "type": "block",
    "description": "Fixed-Head",
    "typecategory": "Blocks",
    "description": "Fixed-head boundary",
    "icon": {
      "filename": "fixedhead.png"
    },
    "Storage": {
      "type": "balance",
      "massbalance": "true",
      "flow": "flow",
      "default": "100000",
      "includeinoutput": "true",
      "description": "Initial Storage; Storage",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m~^3;ft~^3",
      "estimate": "true"
    },
    "head": {
      "type": "value",
      "description": "Head",
      "ask_user": "true",
      "includeinoutput": "true",
      "default": "0",
      "delegate": "UnitBox",
      "unit": "m;ft"
    },
    "bottom_elevation": {
      "type": "expression",
      "expression": "head",	
      "ask_user": "false"
    },
    "area": {
      "type": "value",
	  "description": "area",
      "default": "1"
    }
  },
  "time_variable_fixed_head": {
    "type": "block",
    "description": "Time-variable Fixed-Head",
    "typecategory": "Blocks",
    "description": "Time-variable fixed-head boundary",
    "icon": {
    	"filename": "fixedhead_timeseries.png"
    },
    "Storage": {
      "type": "balance",
      "massbalance": "true",
      "flow": "flow",
      "default": "100000",
      "includeinoutput": "true",
      "description": "Initial Storage;Storage",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true"
    },
    "head": {
      "type": "timeseries",
      "includeinoutput": "true",
      "description": "Head (m)",
      "ask_user": "true",
      "delegate": "Browser" 
    }
  },
  "Constituent": {
    "type": "constituent",
    "typecategory": "Constituents",
    "description": "Chemical constituent",
    "icon": {
      "filename": "constituent.png"
    },
    "concentration": {
      "description": "Concentration",
      "type": "expression",
      "expression": "mass/Storage",
      "ask_user": "true",
      "role": "copytoblocks",
      "includeinoutput": "true",
      "delegate": "UnitBox",
      "estimate": "true",
      "unit": "g/m~^3"
    },
    "stoichiometric_constant": {
      "description": "Stoichiometric Constant",
      "type": "expression",
      "ask_user": "true",
      "role": "copytoreactions",
      "includeinoutput": "false",
      "delegate": "expressionEditor"
    },
    "mass": {
      "description": "Mass",
      "type": "balance",
      "massbalance": "true",
      "flow": "masstransfer",
      "inflow": "external_mass_flow_timeseries,external_source,inflow_loading",
      "includeinoutput": "true",
      "ask_user": "false",
      "role": "copytoblocks",
      "initial_value_expression": "concentration*Storage"
    },
    "diffusion_coefficient": {
      "description": "Diffusion Coefficient",
      "type": "value",
      "ask_user": "true",
      "delegate": "UnitBox",
      "estimate": "true",
      "unit": "m~^2/s;m~^2/day;ft~^2/s;ft~^2/day"
    },
    "advective_masstransfer": {
      "description": "Advective mass transfer rate",
      "type": "expression",
      "expression": "_ups(flow;concentration)",
      "includeinoutput": "true",
      "ask_user": "false",
      "role": "copytolinks"
    },
    "diffusive_masstransfer": {
      "description": "Diffusive mass transfer rate",
      "type": "expression",
      "expression": "diffusion_coefficient*area/length*(concentration.s-concentration.e)",
      "includeinoutput": "true",
      "ask_user": "false",
      "role": "copytolinks"
    },
    "masstransfer": {
      "description": "Total mass transfer rate",
      "type": "expression",
      "expression": "advective_masstransfer+diffusive_masstransfer",
      "includeinoutput": "true",
      "ask_user": "false",
      "role": "copytolinks"
    },
    "external_mass_flow_timeseries": {
      "type": "timeseries",
      "includeinoutput": "true",
      "description": "External mass flow time-series",
      "ask_user": "true",
      "role": "copytoblocks",
      "delegate": "Browser"
    },
    "inflow_concentration": {
      "type": "timeseries",
      "includeinoutput": "true",
      "description": "Inflow concentration",
      "ask_user": "true",
      "role": "copytoblocks",
      "delegate": "Browser"
    },
    "inflow_loading": {
      "type": "expression",
      "expression": "inflow_concentration*inflow_timeseries",
      "includeinoutput": "false",
      "description": "Inflow loading",
      "role": "copytoblocks"
    },
    "external_source": {
      "type": "source",
      "includeinoutput": "true",
      "description": "External source",
      "ask_user": "true",
      "role": "copytoblocks",
      "delegate": "ComboBox;Sources"
    }
  },
  "Particle": {
    "type": "constituent",
    "typecategory": "Constituents",
    "description": "Solid Particle",
    "icon": {
      "filename": "Particle.png"
    },
    "concentration": {
      "description": "Concentration",
      "type": "expression",
      "expression": "mass/Storage",
      "ask_user": "true",
      "role": "copytoblocks",
      "includeinoutput": "true",
      "delegate": "UnitBox",
      "estimate": "true",
      "unit": "g/m~^3"
    },
    "settling_velocity": {
      "description": "Settling velocity",
      "type": "value",
      "ask_user": "true",
      "delegate": "UnitBox",
      "estimate": "true",
      "unit": "m/s;m/day;ft/s;ft/day"
    },
    "stoichiometric_constant": {
      "description": "Stoichiometric Constant",
      "type": "expression",
      "ask_user": "true",
      "role": "copytoreactions",
      "includeinoutput": "false",
      "delegate": "expressionEditor"
    },
    "mass": {
      "description": "Mass",
      "type": "balance",
      "massbalance": "true",
      "flow": "masstransfer",
      "inflow": "external_mass_flow_timeseries,external_source",
      "includeinoutput": "true",
      "ask_user": "false",
      "role": "copytoblocks",
      "initial_value_expression": "concentration*Storage"
    },
    "masstransfer": {
      "description": "Mass transfer rate",
      "type": "expression",
      "expression": "_ups(flow;concentration)+settling_velocity*area*(_hsd(bottom_elevation.s-bottom_elevation.e-0.0000001)*concentration.s-_hsd(bottom_elevation.e-bottom_elevation.s-0.000001)*concentration.e)",
      "includeinoutput": "true",
      "ask_user": "false",
      "role": "copytolinks"
    },
    "external_mass_flow_timeseries": {
      "type": "timeseries",
      "includeinoutput": "true",
      "description": "External mass flow time-series",
      "ask_user": "true",
      "role": "copytoblocks",
      "delegate": "Browser"
    },
    "external_source": {
      "type": "source",
      "includeinoutput": "true",
      "description": "External source",
      "ask_user": "true",
      "role": "copytoblocks",
      "delegate": "ComboBox;Sources"
    }
  },
  "Reaction": {
    "type": "Reaction",
    "description": "Chemical reaction",
    "typecategory": "Reactions",
    "normalizing_quantity": "Storage",
    "icon": {
      "filename": "reaction.png"
    },
    "rate_expression": {
      "description": "Rate Expression",
      "type": "expression",
      "ask_user": "true",
      "delegate": "expressionEditor"
    }
  },
  "ReactionParameter": {
    "type": "ReactionParameter",
    "description": "Reaction Parameter",
    "typecategory": "Reaction Parameters",
    "icon": {
      "filename": "reactionparameter.png"
    },
    "base_value": {
      "description": "Value",
      "type": "value",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true"
    },
	"value": {
      "description": "Value",
      "type": "expression",
	  "expression": "base_value*Arrhenius_factor^(temperature-reference_temperature)",
      "ask_user": "false",
	  "includeinoutput": "true"
    },
	"reference_temperature": {
      "description": "Reference Temperature",
      "type": "value",
	  "default": "0",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true"
    },
	"temperature": {
      "type": "timeseries",
      "includeinoutput": "true",
      "description": "Temperature time-series (degrees Celsius)",
      "ask_user": "true",
      "delegate": "Browser" 
    },
	"Arrhenius_factor": {
      "description": "Arrhenius temperature correction factor",
      "type": "value",
      "default": "1",
	  "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true"
    }
  }
}
