  "junction_elastic": {
    "type": "block",
    "typecategory": "Blocks",
    "description": "Elastic Junction",
    "icon": {
      "filename": "junction.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String"
    },
    "Storage": {
      "type": "balance",
      "massbalance": "true",
      "flow": "flow",
      "inflow": "Precipitation",
      "includeinoutput": "true",
      "description": "Storage",
      "default": "100000"
    },
    "elevation": {
      "type": "value",
      "description": "Elevation",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft",
      "estimate": "true"
    },
    "elasticity": {
      "type": "value",
      "description": "Elasticity",
      "default": "100",
      "ask_user": "true",
      "delegate": "ValueBox",
      "estimate": "true"
    },
    "head": {
      "type": "expression",
      "expression": "elevation+elasticity*(Storage-100000)",
      "description": "Hydraulc Head",
      "ask_user": "false",
      "includeinoutput": "true"
    }
  },
  "junction": {
    "type": "block",
    "typecategory": "Blocks",
	"description": "Rigid Junction",
    "icon": {
      "filename": "junction_rigid.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String"
    },
    "Storage": {
      "type": "balance",
      "massbalance": "true",
      "flow": "flow",
      "inflow": "Precipitation",
      "includeinoutput": "true",
      "description": "Storage",
      "includeinoutput": "false",
      "default": "1000",
      "rigid": "true"
    },
    "elevation": {
      "type": "value",
      "description": "Elevation",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft",
      "includeinoutput": "true",
      "estimate": "true"
    },
    "head": {
      "type": "expression",
      "expression": "elevation+1*(Storage-1000)",
      "description": "Hydraulc Head",
      "ask_user": "false",
      "includeinoutput": "true",
      "includeinoutput": "true"
    }
  },
  "pump": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Pump with a quadratic head-flow relationship",
    "icon": {
      "filename": "pump.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "head_0": {
      "type": "value",
      "description": "pump head at a zero flow (h0)",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm",
      "estimate": "true",
      "criteria": "head_0>0",
      "warningmessage": "pump head must be positive"
    },
    "alpha": {
      "type": "value",
      "description": "The coefficient alpha in h=h0-alpha*Q^beta",
      "ask_user": "true",
      "delegate": "ValueBox",
      "unit": "m",
      "estimate": "true",
      "criteria": "alpha>0",
      "warningmessage": "The coefficient must be positive"
    },
    "beta": {
      "type": "value",
      "description": "The exponent beta in h=h0-alpha*Q^beta",
      "ask_user": "true",
      "delegate": "ValueBox",
      "unit": "m",
      "estimate": "true",
      "default": "2",
      "criteria": "beta>0",
      "warningmessage": "The exponent must be positive"
    },
    "flow": {
      "type": "expression",
      "description": "Flow rate (m~^3/day)",
      "expression": "(_pos(head.s-head.e+head_0)/alpha)^(1/beta)",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    },
    "power": {
      "type": "expression",
      "description": "Power (Watts)",
      "expression": "(9810/86400)*_pos(head.s-head.e+head_0)*flow",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    },
    "color": {
      "type": "string",
      "default": "0,0,0"
    }
  },
  "pump_with_timeseries_control": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Pump with time series control",
    "icon": {
    "filename": "pump_with_time_series_control.png"
    },
    "head_0": {
      "type": "value",
      "description": "pump head at a zero flow (h0)",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm",
      "estimate": "true",
      "criteria": "head_0>0",
      "warningmessage": "pump head must be positive"
    },
    "alpha": {
      "type": "value",
      "description": "The coefficient alpha in h=h0-alpha*Q^beta",
      "ask_user": "true",
      "delegate": "ValueBox",
      "unit": "m",
      "estimate": "true",
      "criteria": "alpha>0",
      "warningmessage": "The coefficient must be positive"
    },
    "beta": {
      "type": "value",
      "description": "The exponent beta in h=h0-alpha*Q^beta",
      "ask_user": "true",
      "delegate": "ValueBox",
      "unit": "m",
      "estimate": "true",
      "default": "2",
      "criteria": "beta>0",
      "warningmessage": "The exponent must be positive"
    },
    "coefficient_time_series": {
      "type": "timeseries",
      "includeinoutput": "true",
      "description": "Coefficient time-series",
      "ask_user": "true",
      "delegate": "Browser" 
    },
    "flow": {
      "type": "expression",
      "description": "Flow rate (m~^3/day)",
      "expression": "coefficient_time_series*((_pos(head.s-head.e+head_0)/alpha)^(1/beta))",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    },
    "power": {
      "type": "expression",
      "description": "Power (Watts)",
      "expression": "(9810/86400)*_pos(head.s-head.e+head_0)*flow",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    },
    "color": {
      "type": "string",
      "default": "0,0,0"
    }
  },
  "residential_user": {
    "type": "block",
    "typecategory": "Blocks",
    "description" : "User",    
    "icon": {
      "filename": "residential.png"
    },
    "Storage": {
      "type": "balance",
      "massbalance": "true",
      "flow": "flow",
      "inflow": "net_use_rate",
      "includeinoutput": "false",
      "description": "Initial Storage;Storage",
      "default":"1000"
    },
    "use_rate": {
      "type": "timeseries",
      "includeinoutput": "true",
      "description": "Use rate time series",
      "ask_user": "true",
      "delegate": "Browser"
    },
	"constant_use_rate": {
      "type": "value",
      "includeinoutput": "true",
      "description": "Constant use rate",
      "ask_user": "true",
	  "unit": "m~^3/day;ft~^3/day",
      "delegate": "UnitBox"
    },
    "net_use_rate": {
      "type": "expression",
      "expression": "-(use_rate+constant_use_rate)",
      "includeinoutput": "false",
      "description": "Net production",
      "ask_user": "false"
    }
  },
  "Flow_to_user": {
    "type": "link",
    "typecategory": "Connectors",
    "description" : "Flow link to user",    
    "icon": {
      "filename": "arrow_to_user.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "flow": {
      "type": "expression",
      "expression": "use_rate.e+constant_use_rate.e",
      "includeinoutput": "true",
      "description": "Flow",
      "applylimit": "true",
      "description": "Flow rate"
    }
  }
}
