{
  "Tank": {
    "type": "block",
    "typecategory": "Blocks",
    "icon": {
      "filename": "watertank.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String"
    },
    "Storage": {
      "type": "balance",
      "massbalance": "true",
      "flow": "flow",
      "includeinoutput": "true",
      "description": "Initial Storage;Storage",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m~^3;ft~^3",
      "estimate": "true"
    },
    "area": {
      "type": "value",
      "description": "Base Area",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m~^2;ft~^2",
      "estimate": "true",
      "criteria": "area>0",
      "warningmessage": "Base area must be positive"
    },
    "bottom_elevation": {
      "type": "value",
      "description": "Bottom Elevation",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft",
      "estimate": "true"
    },
    "depth": {
      "type": "expression",
      "expression": "Storage/area",
      "description": "Water Depth",
      "ask_user": "false",
      "delegate": "UnitBox",
      "unit": "m;ft;cm",
      "includeinoutput": "true"
    },
    "head": {
      "type": "expression",
      "expression": "depth+bottom_elevation",      
      "description": "Hydraulc Head",
      "ask_user": "false",
      "includeinoutput": "true"
    }
  },
  "pipe_2w": {
    "type": "link",
    "typecategory": "Connectors",
    "description": "Pipe (Hazen-Williams Equation, 2 way)",
    "icon": {
      "filename": "pipe.png"
    },
    "name": {
      "description": "Name",
      "type": "string",
      "ask_user": "true",
      "delegate": "String" 
    },
    "diameter": {
      "type": "value",
      "description": "Diameter",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft;cm",
      "estimate": "true",
      "criteria": "diameter>0",
      "warningmessage": "diameter must be positive"
    },
    "hz_coeff": {
      "type": "value",
      "description": "Hazen-Williams Coefficient (SI)",
      "ask_user": "true",
      "delegate": "ValueBox",
      "unit": "m",
      "estimate": "true",
      "default": "100",
      "criteria": "hz_coeff>0",
      "warningmessage": "diameter must be positive"
    },
    "length": {
      "type": "value",
      "description": "Pipe length",
      "ask_user": "true",
      "delegate": "UnitBox",
      "unit": "m;ft",
      "estimate": "true",
      "criteria": "length>0",
      "warningmessage": "lenght must be positive"
    },
    "hydraulic_gradient": {
      "type": "expression",
      "description": "Hydraulic Gradient",
      "expression": "(head.e-head.s)/length",
      "includeinoutput": "true",
      "ask_user": "false"
    },
    "flow": {
      "type": "expression",
      "description": "Flow rate (m~^3/day)",
      "expression": "24055.44*hz_coeff*diameter^2.63*_sgn(head.s-head.e)*_mbs((head.s-head.e)/length;0.00001)*_abs((head.s-head.e)/length)^0.54",
      "includeinoutput": "true",
      "applylimit": "true",
      "ask_user": "false"
    },
    "color": {
      "type": "string",
      "default": "0,0,200"
    }
  },
  "pipe_1w": {
    "type": "link",
